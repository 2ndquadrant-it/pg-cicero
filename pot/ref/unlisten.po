msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:111(programlisting)
#, no-wrap
msgid "\nUNLISTEN virtual;\nNOTIFY virtual;\n-- no NOTIFY event is received\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:107(para)
msgid "Once <command>UNLISTEN</command> has been executed, further <command>NOTIFY</command> messages will be ignored: <placeholder-1/>"
msgstr ""

