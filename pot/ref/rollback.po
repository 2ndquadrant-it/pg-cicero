msgid "\nROLLBACK;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:71(para)
msgid "To abort all changes: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:80(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:82(para)
msgid "The SQL standard only specifies the two forms <literal>ROLLBACK</literal> and <literal>ROLLBACK WORK</literal>. Otherwise, this command is fully conforming."
msgstr ""

