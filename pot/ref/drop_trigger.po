msgid "DROP TRIGGER"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:17(refpurpose) 
msgid "remove a trigger"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:26(replaceable) /tmp/cicero-lyhk/ref/drop_trigger.xml:56(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:26(replaceable) /tmp/cicero-lyhk/ref/drop_trigger.xml:65(replaceable) 
msgid "table"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP TRIGGER [ IF EXISTS ] <placeholder-1/> ON <placeholder-2/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:33(para) 
msgid "<command>DROP TRIGGER</command> removes an existing trigger definition. To execute this command, the current user must be the owner of the table for which the trigger is defined."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:46(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:48(para) 
msgid "Do not throw an error if the trigger does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:58(para) 
msgid "The name of the trigger to remove."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:67(para) 
msgid "The name (optionally schema-qualified) of the table for which the trigger is defined."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:75(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:77(para) 
msgid "Automatically drop objects that depend on the trigger."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:84(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:86(para) 
msgid "Refuse to drop the trigger if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:96(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:102(programlisting) 
#, no-wrap
msgid "\nDROP TRIGGER if_dist_exists ON films;\n"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:98(para) 
msgid "Destroy the trigger <literal>if_dist_exists</literal> on the table <literal>films</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:108(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_trigger.xml:110(para) 
msgid "The <command>DROP TRIGGER</command> statement in <productname>PostgreSQL</productname> is incompatible with the SQL standard. In the SQL standard, trigger names are not local to tables, so the command is simply <literal>DROP TRIGGER <replaceable>name</replaceable></literal>."
msgstr ""

