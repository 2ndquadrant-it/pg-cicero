#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:25(synopsis) 
#, no-wrap
msgid "\nCREATE TEXT SEARCH CONFIGURATION <placeholder-1/> (\n    PARSER = <placeholder-2/> |\n    COPY = <placeholder-3/>\n)\n"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:34(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:36(para) 
msgid "<command>CREATE TEXT SEARCH CONFIGURATION</command> creates a new text search configuration. A text search configuration specifies a text search parser that can divide a string into tokens, plus dictionaries that can be used to determine which tokens are of interest for searching."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:43(para) 
msgid "If only the parser is specified, then the new text search configuration initially has no mappings from token types to dictionaries, and therefore will ignore all words. Subsequent <command>ALTER TEXT SEARCH CONFIGURATION</command> commands must be used to create mappings to make the configuration useful. Alternatively, an existing text search configuration can be copied."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:52(para) 
msgid "If a schema name is given then the text search configuration is created in the specified schema. Otherwise it is created in the current schema."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:57(para) 
msgid "The user who defines a text search configuration becomes its owner."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:61(para) 
msgid "Refer to <xref linkend=\"textsearch\"/> for further information."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:67(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:73(para) 
msgid "The name of the text search configuration to be created. The name can be schema-qualified."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:83(para) 
msgid "The name of the text search parser to use for this configuration."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:92(para) 
msgid "The name of an existing text search configuration to copy."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:101(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:103(para) 
msgid "The <literal>PARSER</literal> and <literal>COPY</literal> options are mutually exclusive, because when an existing configuration is copied, its parser selection is copied too."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:112(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsconfig.xml:114(para) 
msgid "There is no <command>CREATE TEXT SEARCH CONFIGURATION</command> statement in the SQL standard."
msgstr ""

