msgid "DROP TEXT SEARCH TEMPLATE"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:17(refpurpose) 
msgid "remove a text search template"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:26(replaceable) /tmp/cicero-RKYt/ref/drop_tstemplate.xml:55(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP TEXT SEARCH TEMPLATE [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:33(para) 
msgid "<command>DROP TEXT SEARCH TEMPLATE</command> drops an existing text search template. You must be a superuser to use this command."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:47(para) 
msgid "Do not throw an error if the text search template does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:57(para) 
msgid "The name (optionally schema-qualified) of an existing text search template."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:65(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:67(para) 
msgid "Automatically drop objects that depend on the text search template."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:74(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:76(para) 
msgid "Refuse to drop the text search template if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:86(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:91(programlisting) 
#, no-wrap
msgid "\nDROP TEXT SEARCH TEMPLATE thesaurus;\n"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:88(para) 
msgid "Remove the text search template <literal>thesaurus</literal>: <placeholder-1/> This command will not succeed if there are any existing text search dictionaries that use the template. Add <literal>CASCADE</literal> to drop such dictionaries along with the template."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:102(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_tstemplate.xml:104(para) 
msgid "There is no <command>DROP TEXT SEARCH TEMPLATE</command> statement in the SQL standard."
msgstr ""

