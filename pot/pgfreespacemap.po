#, no-wrap
msgid "\npostgres=# SELECT * FROM pg_freespace('foo');\n blkno | avail \n-------+-------\n     0 |     0\n     1 |     0\n     2 |     0\n     3 |    32\n     4 |   704\n     5 |   704\n     6 |   704\n     7 |  1216\n     8 |   704\n     9 |   704\n    10 |   704\n    11 |   704\n    12 |   704\n    13 |   704\n    14 |   704\n    15 |   704\n    16 |   704\n    17 |   704\n    18 |   704\n    19 |  3648\n(20 rows)\n\npostgres=# SELECT * FROM pg_freespace('foo', 7);\n pg_freespace \n--------------\n         1216\n(1 row)\n"
msgstr ""

