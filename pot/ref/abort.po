msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:16(refpurpose) 
msgid "abort the current transaction"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:24(synopsis) 
#, no-wrap
msgid "\nABORT [ WORK | TRANSACTION ]\n"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:32(para) 
msgid "<command>ABORT</command> rolls back the current transaction and causes all the updates made by the transaction to be discarded. This command is identical in behavior to the standard <acronym>SQL</acronym> command <xref linkend=\"sql-rollback\"/>, and is present only for historical reasons."
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:43(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:47(literal) 
msgid "WORK"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:48(literal) 
msgid "TRANSACTION"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:50(para) 
msgid "Optional key words. They have no effect."
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:59(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:61(para) 
msgid "Use <xref linkend=\"sql-commit\"/> to successfully terminate a transaction."
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:66(para) 
msgid "Issuing <command>ABORT</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:73(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:77(programlisting) 
#, no-wrap
msgid "\nABORT;\n"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:75(para) 
msgid "To abort all changes: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:83(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-AgyC/ref/abort.xml:85(para) 
msgid "This command is a <productname>PostgreSQL</productname> extension present for historical reasons. <command>ROLLBACK</command> is the equivalent standard SQL command."
msgstr ""

