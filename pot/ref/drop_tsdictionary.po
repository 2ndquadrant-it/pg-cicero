msgid "DROP TEXT SEARCH DICTIONARY"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:16(refpurpose) 
msgid "remove a text search dictionary"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:25(replaceable) /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:55(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP TEXT SEARCH DICTIONARY [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:32(para) 
msgid "<command>DROP TEXT SEARCH DICTIONARY</command> drops an existing text search dictionary. To execute this command you must be the owner of the dictionary."
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:47(para) 
msgid "Do not throw an error if the text search dictionary does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:57(para) 
msgid "The name (optionally schema-qualified) of an existing text search dictionary."
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:65(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:67(para) 
msgid "Automatically drop objects that depend on the text search dictionary."
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:74(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:76(para) 
msgid "Refuse to drop the text search dictionary if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:86(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:91(programlisting) 
#, no-wrap
msgid "\nDROP TEXT SEARCH DICTIONARY english;\n"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:88(para) 
msgid "Remove the text search dictionary <literal>english</literal>: <placeholder-1/> This command will not succeed if there are any existing text search configurations that use the dictionary. Add <literal>CASCADE</literal> to drop such configurations along with the dictionary."
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:102(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-gTQe/ref/drop_tsdictionary.xml:104(para) 
msgid "There is no <command>DROP TEXT SEARCH DICTIONARY</command> statement in the SQL standard."
msgstr ""

