msgid "DROP TEXT SEARCH DICTIONARY"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:17(refpurpose) 
msgid "remove a text search dictionary"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:26(replaceable) /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:56(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP TEXT SEARCH DICTIONARY [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:33(para) 
msgid "<command>DROP TEXT SEARCH DICTIONARY</command> drops an existing text search dictionary. To execute this command you must be the owner of the dictionary."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:46(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:48(para) 
msgid "Do not throw an error if the text search dictionary does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:58(para) 
msgid "The name (optionally schema-qualified) of an existing text search dictionary."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:66(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:68(para) 
msgid "Automatically drop objects that depend on the text search dictionary."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:75(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:77(para) 
msgid "Refuse to drop the text search dictionary if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:87(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:92(programlisting) 
#, no-wrap
msgid "\nDROP TEXT SEARCH DICTIONARY english;\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:89(para) 
msgid "Remove the text search dictionary <literal>english</literal>: <placeholder-1/> This command will not succeed if there are any existing text search configurations that use the dictionary. Add <literal>CASCADE</literal> to drop such configurations along with the dictionary."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:103(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_tsdictionary.xml:105(para) 
msgid "There is no <command>DROP TEXT SEARCH DICTIONARY</command> statement in the SQL standard."
msgstr ""

