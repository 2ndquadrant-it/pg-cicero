msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:56(para)
msgid "<application>createlang</application> accepts the following command-line arguments: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:132(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:133(replaceable)
msgid "host"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:132(option)
msgid "-h <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:133(option)
msgid "--host <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:135(para)
msgid "Specifies the host name of the machine on which the server is running. If the value begins with a slash, it is used as the directory for the Unix domain socket."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:145(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:146(replaceable)
msgid "port"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:145(option)
msgid "-p <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:146(option)
msgid "--port <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:148(para)
msgid "Specifies the TCP port or local Unix domain socket file extension on which the server is listening for connections."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:157(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:158(replaceable)
msgid "username"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:157(option)
msgid "-U <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:158(option)
msgid "--username <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:160(para)
msgid "User name to connect as."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:167(option)
msgid "-w"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:168(option)
msgid "--no-password"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:170(para)
msgid "Never issue a password prompt. If the server requires password authentication and a password is not available by other means such as a <filename>.pgpass</filename> file, the connection attempt will fail. This option can be useful in batch jobs and scripts where no user is present to enter a password."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:182(option)
msgid "-W"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:183(option)
msgid "--password"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:185(para)
msgid "Force <application>createlang</application> to prompt for a password before connecting to a database."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:190(para)
msgid "This option is never essential, since <application>createlang</application> will automatically prompt for a password if the server demands password authentication. However, <application>createlang</application> will waste a connection attempt finding out that the server wants a password. In some cases it is worth typing <option>-W</option> to avoid the extra connection attempt."
msgstr ""

