msgid "DROP FOREIGN TABLE"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:7(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:8(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:13(refpurpose) 
msgid "remove a foreign table"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:22(replaceable) /tmp/cicero-YDWB/ref/drop_foreign_table.xml:50(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:21(synopsis) 
#, no-wrap
msgid "\nDROP FOREIGN TABLE [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:27(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:29(para) 
msgid "<command>DROP FOREIGN TABLE</command> removes a foreign table. Only the owner of a foreign table can remove it."
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:36(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:40(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:42(para) 
msgid "Do not throw an error if the foreign table does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:52(para) 
msgid "The name (optionally schema-qualified) of the foreign table to drop."
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:59(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:61(para) 
msgid "Automatically drop objects that depend on the foreign table (such as views)."
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:69(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:71(para) 
msgid "Refuse to drop the foreign table if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:81(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:87(programlisting) 
#, no-wrap
msgid "\nDROP FOREIGN TABLE films, distributors;\n"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:83(para) 
msgid "To destroy two foreign tables, <literal>films</literal> and <literal>distributors</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:93(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-YDWB/ref/drop_foreign_table.xml:95(para) 
msgid "This command conforms to the ISO/IEC 9075-9 (SQL/MED), except that the standard only allows one foreign table to be dropped per command, and apart from the <literal>IF EXISTS</literal> option, which is a <productname>PostgreSQL</productname> extension."
msgstr ""

