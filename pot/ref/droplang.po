#: /tmp/cicero-cjWZ/ref/droplang.xml:68(para) 
msgid "<application>droplang</application> accepts the following command line arguments: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:143(replaceable) /tmp/cicero-cjWZ/ref/droplang.xml:144(replaceable) 
msgid "host"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:143(option) 
msgid "-h <placeholder-1/>"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:144(option) 
msgid "--host=<placeholder-1/>"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:146(para) 
msgid "Specifies the host name of the machine on which the server is running. If host begins with a slash, it is used as the directory for the Unix domain socket."
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:156(replaceable) /tmp/cicero-cjWZ/ref/droplang.xml:157(replaceable) 
msgid "port"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:156(option) 
msgid "-p <placeholder-1/>"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:157(option) 
msgid "--port=<placeholder-1/>"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:159(para) 
msgid "Specifies the Internet TCP/IP port or local Unix domain socket file extension on which the server is listening for connections."
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:168(replaceable) /tmp/cicero-cjWZ/ref/droplang.xml:169(replaceable) 
msgid "username"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:168(option) 
msgid "-U <placeholder-1/>"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:169(option) 
msgid "--username=<placeholder-1/>"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:171(para) 
msgid "User name to connect as."
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:178(option) 
msgid "-w"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:179(option) 
msgid "--no-password"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:181(para) 
msgid "Never issue a password prompt. If the server requires password authentication and a password is not available by other means such as a <filename>.pgpass</filename> file, the connection attempt will fail. This option can be useful in batch jobs and scripts where no user is present to enter a password."
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:193(option) 
msgid "-W"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:194(option) 
msgid "--password"
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:196(para) 
msgid "Force <application>droplang</application> to prompt for a password before connecting to a database."
msgstr ""

#: /tmp/cicero-cjWZ/ref/droplang.xml:201(para) 
msgid "This option is never essential, since <application>droplang</application> will automatically prompt for a password if the server demands password authentication. However, <application>droplang</application> will waste a connection attempt finding out that the server wants a password. In some cases it is worth typing <option>-W</option> to avoid the extra connection attempt."
msgstr ""

