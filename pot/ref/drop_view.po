msgid "DROP VIEW"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:17(refpurpose) 
msgid "remove a view"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:26(replaceable) /tmp/cicero-QnGh/ref/drop_view.xml:54(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP VIEW [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:33(para) 
msgid "<command>DROP VIEW</command> drops an existing view. To execute this command you must be the owner of the view."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:44(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:46(para) 
msgid "Do not throw an error if the view does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:56(para) 
msgid "The name (optionally schema-qualified) of the view to remove."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:63(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:65(para) 
msgid "Automatically drop objects that depend on the view (such as other views)."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:73(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:75(para) 
msgid "Refuse to drop the view if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:85(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:89(programlisting) 
#, no-wrap
msgid "\nDROP VIEW kinds;\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:87(para) 
msgid "This command will remove the view called <literal>kinds</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:95(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_view.xml:97(para) 
msgid "This command conforms to the SQL standard, except that the standard only allows one view to be dropped per command, and apart from the <literal>IF EXISTS</literal> option, which is a <productname>PostgreSQL</productname> extension."
msgstr ""

