msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:17(refpurpose) 
msgid "abort the current transaction"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:25(synopsis) 
#, no-wrap
msgid "\nABORT [ WORK | TRANSACTION ]\n"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:33(para) 
msgid "<command>ABORT</command> rolls back the current transaction and causes all the updates made by the transaction to be discarded. This command is identical in behavior to the standard <acronym>SQL</acronym> command <xref linkend=\"sql-rollback\"/>, and is present only for historical reasons."
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:44(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:48(literal) 
msgid "WORK"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:49(literal) 
msgid "TRANSACTION"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:51(para) 
msgid "Optional key words. They have no effect."
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:60(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:62(para) 
msgid "Use <xref linkend=\"sql-commit\"/> to successfully terminate a transaction."
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:67(para) 
msgid "Issuing <command>ABORT</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:74(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:78(programlisting) 
#, no-wrap
msgid "\nABORT;\n"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:76(para) 
msgid "To abort all changes: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:84(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-LTnd/ref/abort.xml:86(para) 
msgid "This command is a <productname>PostgreSQL</productname> extension present for historical reasons. <command>ROLLBACK</command> is the equivalent standard SQL command."
msgstr ""

