#: /tmp/cicero-hjxh/ref/clusterdb.xml:66(para) 
msgid "<application>clusterdb</application> accepts the following command-line arguments: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:166(replaceable) /tmp/cicero-hjxh/ref/clusterdb.xml:167(replaceable) 
msgid "host"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:166(option) 
msgid "-h <placeholder-1/>"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:167(option) 
msgid "--host=<placeholder-1/>"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:169(para) 
msgid "Specifies the host name of the machine on which the server is running. If the value begins with a slash, it is used as the directory for the Unix domain socket."
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:178(replaceable) /tmp/cicero-hjxh/ref/clusterdb.xml:179(replaceable) 
msgid "port"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:178(option) 
msgid "-p <placeholder-1/>"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:179(option) 
msgid "--port=<placeholder-1/>"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:181(para) 
msgid "Specifies the TCP port or local Unix domain socket file extension on which the server is listening for connections."
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:190(replaceable) /tmp/cicero-hjxh/ref/clusterdb.xml:191(replaceable) 
msgid "username"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:190(option) 
msgid "-U <placeholder-1/>"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:191(option) 
msgid "--username=<placeholder-1/>"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:193(para) 
msgid "User name to connect as."
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:200(option) 
msgid "-w"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:201(option) 
msgid "--no-password"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:203(para) 
msgid "Never issue a password prompt. If the server requires password authentication and a password is not available by other means such as a <filename>.pgpass</filename> file, the connection attempt will fail. This option can be useful in batch jobs and scripts where no user is present to enter a password."
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:215(option) 
msgid "-W"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:216(option) 
msgid "--password"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:218(para) 
msgid "Force <application>clusterdb</application> to prompt for a password before connecting to a database."
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:223(para) 
msgid "This option is never essential, since <application>clusterdb</application> will automatically prompt for a password if the server demands password authentication. However, <application>clusterdb</application> will waste a connection attempt finding out that the server wants a password. In some cases it is worth typing <option>-W</option> to avoid the extra connection attempt."
msgstr ""

