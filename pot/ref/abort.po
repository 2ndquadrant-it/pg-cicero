msgid "\nABORT [ WORK | TRANSACTION ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:32(para)
msgid "<command>ABORT</command> rolls back the current transaction and causes all the updates made by the transaction to be discarded. This command is identical in behavior to the standard <acronym>SQL</acronym> command <xref linkend=\"SQL-ROLLBACK\"/>, and is present only for historical reasons."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:43(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:47(literal)
msgid "WORK"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:48(literal)
msgid "TRANSACTION"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:50(para)
msgid "Optional key words. They have no effect."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:59(title)
msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:61(para)
msgid "Use <xref linkend=\"SQL-COMMIT\"/> to successfully terminate a transaction."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:66(para)
msgid "Issuing <command>ABORT</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/abort.xml:73(title)
msgid "Examples"
msgstr ""

