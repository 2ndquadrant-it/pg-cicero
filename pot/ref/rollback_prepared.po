#, no-wrap
msgid "\nROLLBACK PREPARED <placeholder-1/>\n"
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:33(para) 
msgid "<command>ROLLBACK PREPARED</command> rolls back a transaction that is in prepared state."
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:46(para) 
msgid "The transaction identifier of the transaction that is to be rolled back."
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:56(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:58(para) 
msgid "To roll back a prepared transaction, you must be either the same user that executed the transaction originally, or a superuser. But you do not have to be in the same session that executed the transaction."
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:64(para) 
msgid "This command cannot be executed inside a transaction block. The prepared transaction is rolled back immediately."
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:69(para) 
msgid "All currently available prepared transactions are listed in the <link linkend=\"view-pg-prepared-xacts\"><structname>pg_prepared_xacts</structname></link> system view."
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:77(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:82(programlisting) 
#, no-wrap
msgid "\nROLLBACK PREPARED 'foobar';\n"
msgstr ""

#: /tmp/cicero-lkRf/ref/rollback_prepared.xml:78(para) 
msgid "Roll back the transaction identified by the transaction identifier <literal>foobar</literal>: <placeholder-1/>"
msgstr ""

