#: /tmp/cicero-EpiG/ref/end.xml:48(literal) 
msgid "TRANSACTION"
msgstr ""

#: /tmp/cicero-EpiG/ref/end.xml:50(para) 
msgid "Optional key words. They have no effect."
msgstr ""

#: /tmp/cicero-EpiG/ref/end.xml:59(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-EpiG/ref/end.xml:61(para) 
msgid "Use <xref linkend=\"sql-rollback\"/> to abort a transaction."
msgstr ""

#: /tmp/cicero-EpiG/ref/end.xml:66(para) 
msgid "Issuing <command>END</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

