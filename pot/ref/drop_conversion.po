#: /tmp/cicero-BGQN/ref/drop_conversion.xml:21(synopsis) 
#, no-wrap
msgid "\nDROP CONVERSION [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:27(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:29(para) 
msgid "<command>DROP CONVERSION</command> removes a previously defined conversion. To be able to drop a conversion, you must own the conversion."
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:36(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:40(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:42(para) 
msgid "Do not throw an error if the conversion does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:53(para) 
msgid "The name of the conversion. The conversion name can be schema-qualified."
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:61(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:62(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:65(para) 
msgid "These key words do not have any effect, since there are no dependencies on conversions."
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:75(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:79(programlisting) 
#, no-wrap
msgid "\nDROP CONVERSION myname;\n"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:77(para) 
msgid "To drop the conversion named <literal>myname</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:85(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-BGQN/ref/drop_conversion.xml:87(para) 
msgid "There is no <command>DROP CONVERSION</command> statement in the SQL standard, but a <command>DROP TRANSLATION</command> statement that goes along with the <command>CREATE TRANSLATION</command> statement that is similar to the <command>CREATE CONVERSION</command> statement in PostgreSQL."
msgstr ""

