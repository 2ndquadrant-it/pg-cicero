msgid "DROP LANGUAGE"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:16(refpurpose) 
msgid "remove a procedural language"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:25(replaceable) /tmp/cicero-yKZa/ref/drop_language.xml:64(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP [ PROCEDURAL ] LANGUAGE [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:32(para) 
msgid "<command>DROP LANGUAGE</command> removes the definition of a previously registered procedural language. You must be a superuser or the owner of the language to use <command>DROP LANGUAGE</command>."
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:39(para) 
msgid "As of <productname>PostgreSQL</productname> 9.1, most procedural languages have been made into <quote>extensions</quote>, and should therefore be removed with <xref linkend=\"sql-dropextension\"/> not <command>DROP LANGUAGE</command>."
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:49(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:54(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:56(para) 
msgid "Do not throw an error if the language does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:66(para) 
msgid "The name of an existing procedural language. For backward compatibility, the name can be enclosed by single quotes."
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:74(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:76(para) 
msgid "Automatically drop objects that depend on the language (such as functions in the language)."
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:84(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:86(para) 
msgid "Refuse to drop the language if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:96(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:102(programlisting) 
#, no-wrap
msgid "\nDROP LANGUAGE plsample;\n"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:98(para) 
msgid "This command removes the procedural language <literal>plsample</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:108(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-yKZa/ref/drop_language.xml:110(para) 
msgid "There is no <command>DROP LANGUAGE</command> statement in the SQL standard."
msgstr ""

