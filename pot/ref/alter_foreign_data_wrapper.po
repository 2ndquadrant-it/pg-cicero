#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:24(synopsis)
#, no-wrap
msgid "\nALTER FOREIGN DATA WRAPPER <placeholder-1/>\n    [ VALIDATOR <placeholder-2/> | NO VALIDATOR ]\n    [ OPTIONS ( [ ADD | SET | DROP ] <placeholder-3/> ['<placeholder-4/>'] [, ... ]) ]\nALTER FOREIGN DATA WRAPPER <placeholder-5/> OWNER TO <placeholder-6/>\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:33(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:35(para)
msgid "<command>ALTER FOREIGN DATA WRAPPER</command> changes the definition of a foreign-data wrapper. The first form of the command changes the library or the generic options of the foreign-data wrapper (at least one clause is required). The second form changes the owner of the foreign-data wrapper."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:43(para)
msgid "Only superusers can alter foreign-data wrappers. Additionally, only superusers can own foreign-data wrappers."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:50(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:56(para)
msgid "The name of an existing foreign-data wrapper."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:63(literal)
msgid "VALIDATOR <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:65(para)
msgid "Specifies a new foreign-data wrapper validator function."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:69(para)
msgid "Note that it is possible that after changing the validator the options to the foreign-data wrapper, servers, and user mappings have become invalid. It is up to the user to make sure that these options are correct before using the foreign-data wrapper."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:80(literal)
msgid "NO VALIDATOR"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:82(para)
msgid "This is used to specify that the foreign-data wrapper should no longer have a validator function."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:90(literal)
msgid "OPTIONS ( [ ADD | SET | DROP ] <placeholder-1/> ['<placeholder-2/>'] [, ... ] )"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:92(para)
msgid "Change options for the foreign-data wrapper. <literal>ADD</literal>, <literal>SET</literal>, and <literal>DROP</literal> specify the action to be performed. <literal>ADD</literal> is assumed if no operation is explicitly specified. Option names must be unique; names and values are also validated using the foreign data wrapper library."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:106(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:111(programlisting)
#, no-wrap
msgid "\nALTER FOREIGN DATA WRAPPER dbi OPTIONS (ADD foo '1', DROP 'bar');\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:108(para)
msgid "Change a foreign-data wrapper <literal>dbi</literal>, add option <literal>foo</literal>, drop <literal>bar</literal>: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:119(programlisting)
#, no-wrap
msgid "\nALTER FOREIGN DATA WRAPPER dbi VALIDATOR bob.myvalidator;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:116(para)
msgid "Change the foreign-data wrapper <literal>dbi</literal> validator to <literal>bob.myvalidator</literal>: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:126(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_foreign_data_wrapper.xml:128(para)
msgid "<command>ALTER FOREIGN DATA WRAPPER</command> conforms to ISO/IEC 9075-9 (SQL/MED). The standard does not specify the <literal> VALIDATOR</literal> and <literal>OWNER TO</literal> variants of the command."
msgstr ""

