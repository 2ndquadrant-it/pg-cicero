#, no-wrap
msgid "\nCREATE OPERATOR CLASS <placeholder-1/> [ DEFAULT ] FOR TYPE <placeholder-2/>\n  USING <placeholder-3/> [ FAMILY <placeholder-4/> ] AS\n  {  OPERATOR <placeholder-5/> <placeholder-6/> [ ( <placeholder-7/>, <placeholder-8/> ) ] [ FOR SEARCH | FOR ORDER BY <placeholder-9/> ]\n   | FUNCTION <placeholder-10/> [ ( <placeholder-11/> [ , <placeholder-12/> ] ) ] <placeholder-13/> ( <placeholder-14/> [, ...] )\n   | STORAGE <placeholder-15/>\n  } [, ... ]\n"
