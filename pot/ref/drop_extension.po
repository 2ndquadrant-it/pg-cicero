msgid "DROP EXTENSION"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:17(refpurpose) 
msgid "remove an extension"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:26(replaceable) /tmp/cicero-EpiG/ref/drop_extension.xml:59(replaceable) 
msgid "extension_name"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP EXTENSION [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:33(para) 
msgid "<command>DROP EXTENSION</command> removes extensions from the database. Dropping an extension causes its component objects to be dropped as well."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:38(para) 
msgid "You must own the extension to use <command>DROP EXTENSION</command>."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:44(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:49(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:51(para) 
msgid "Do not throw an error if the extension does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:61(para) 
msgid "The name of an installed extension."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:68(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:70(para) 
msgid "Automatically drop objects that depend on the extension."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:77(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:79(para) 
msgid "Refuse to drop the extension if any objects depend on it (other than its own member objects and other extensions listed in the same <command>DROP</command> command). This is the default."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:90(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:95(programlisting) 
#, no-wrap
msgid "\nDROP EXTENSION hstore;\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:92(para) 
msgid "To remove the extension <literal>hstore</literal> from the current database: <placeholder-1/> This command will fail if any of <literal>hstore</literal>'s objects are in use in the database, for example if any tables have columns of the <type>hstore</type> type. Add the <literal>CASCADE</literal> option to forcibly remove those dependent objects as well."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:106(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_extension.xml:108(para) 
msgid "<command>DROP EXTENSION</command> is a <productname>PostgreSQL</productname> extension."
msgstr ""

