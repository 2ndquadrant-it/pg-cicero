msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:43(para)
msgid "<application>createlang</application> is a utility for adding a new programming language to a <productname>PostgreSQL</productname> database. <application>createlang</application> is just a wrapper around the <xref linkend=\"sql-createlanguage\"/> command."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:54(title)
msgid "Options"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:63(para)
msgid "Specifies the name of the procedural programming language to be defined."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:71(optional)
msgid "-d"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:72(optional)
msgid "--dbname"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:74(para)
msgid "Specifies the database to which the language should be added. The default is to use the database with the same name as the current system user."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:83(option)
msgid "-e"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:84(option)
msgid "--echo"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:86(para)
msgid "Display SQL commands as they are executed."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:96(para)
msgid "Show a list of already installed languages in the target database."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:103(option)
msgid "-V"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:104(option)
msgid "--version"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/createlang.xml:106(para)
msgid "Print the <application>createlang</application> version and exit."
msgstr ""

