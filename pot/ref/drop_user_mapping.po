msgid "server_name"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:24(synopsis)
#, no-wrap
msgid "\nDROP USER MAPPING [ IF EXISTS ] FOR { <placeholder-1/> | USER | CURRENT_USER | PUBLIC } SERVER <placeholder-2/>\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:32(para)
msgid "<command>DROP USER MAPPING</command> removes an existing user mapping from foreign server."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:37(para)
msgid "The owner of a foreign server can drop user mappings for that server for any user. Also, a user can drop a user mapping for his own user name if <literal>USAGE</literal> privilege on the server has been granted to the user."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:46(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:50(literal)
msgid "IF EXISTS"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:52(para)
msgid "Do not throw an error if the user mapping does not exist. A notice is issued in this case."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:62(para)
msgid "User name of the mapping. <literal>CURRENT_USER</literal> and <literal>USER</literal> match the name of the current user. <literal>PUBLIC</literal> is used to match all present and future user names in the system."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:74(para)
msgid "Server name of the user mapping."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:83(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:87(programlisting)
#, no-wrap
msgid "\nDROP USER MAPPING IF EXISTS FOR bob SERVER foo;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:85(para)
msgid "Drop a user mapping <literal>bob</literal>, server <literal>foo</literal> if it exists: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:94(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_user_mapping.xml:96(para)
msgid "<command>DROP USER MAPPING</command> conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal> clause is a <productname>PostgreSQL</productname> extension."
msgstr ""

