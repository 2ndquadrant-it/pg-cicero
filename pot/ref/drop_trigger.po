msgid "DROP TRIGGER"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:16(refpurpose) 
msgid "remove a trigger"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:25(replaceable) /tmp/cicero-pPcb/ref/drop_trigger.xml:55(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:25(replaceable) /tmp/cicero-pPcb/ref/drop_trigger.xml:64(replaceable) 
msgid "table"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP TRIGGER [ IF EXISTS ] <placeholder-1/> ON <placeholder-2/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:32(para) 
msgid "<command>DROP TRIGGER</command> removes an existing trigger definition. To execute this command, the current user must be the owner of the table for which the trigger is defined."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:47(para) 
msgid "Do not throw an error if the trigger does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:57(para) 
msgid "The name of the trigger to remove."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:66(para) 
msgid "The name (optionally schema-qualified) of the table for which the trigger is defined."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:74(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:76(para) 
msgid "Automatically drop objects that depend on the trigger."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:83(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:85(para) 
msgid "Refuse to drop the trigger if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:95(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:101(programlisting) 
#, no-wrap
msgid "\nDROP TRIGGER if_dist_exists ON films;\n"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:97(para) 
msgid "Destroy the trigger <literal>if_dist_exists</literal> on the table <literal>films</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:107(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_trigger.xml:109(para) 
msgid "The <command>DROP TRIGGER</command> statement in <productname>PostgreSQL</productname> is incompatible with the SQL standard. In the SQL standard, trigger names are not local to tables, so the command is simply <literal>DROP TRIGGER <replaceable>name</replaceable></literal>."
msgstr ""

