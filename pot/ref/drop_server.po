msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:17(refpurpose) 
msgid "remove a foreign server descriptor"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:26(replaceable) /tmp/cicero-EpiG/ref/drop_server.xml:55(replaceable) 
msgid "server_name"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP SERVER [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:33(para) 
msgid "<command>DROP SERVER</command> removes an existing foreign server descriptor. To execute this command, the current user must be the owner of the server."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:47(para) 
msgid "Do not throw an error if the server does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:57(para) 
msgid "The name of an existing server."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:64(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:66(para) 
msgid "Automatically drop objects that depend on the server (such as user mappings)."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:74(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:76(para) 
msgid "Refuse to drop the server if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:86(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:90(programlisting) 
#, no-wrap
msgid "\nDROP SERVER IF EXISTS foo;\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:88(para) 
msgid "Drop a server <literal>foo</literal> if it exists: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:96(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_server.xml:98(para) 
msgid "<command>DROP SERVER</command> conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal> clause is a <productname>PostgreSQL</productname> extension."
msgstr ""

