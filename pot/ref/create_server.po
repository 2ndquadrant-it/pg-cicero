msgid "\nCREATE SERVER <placeholder-1/> [ TYPE '<placeholder-2/>' ] [ VERSION '<placeholder-3/>' ]\n    FOREIGN DATA WRAPPER <placeholder-4/>\n    [ OPTIONS ( <placeholder-5/> '<placeholder-6/>' [, ... ] ) ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:32(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:34(para)
msgid "<command>CREATE SERVER</command> defines a new foreign server. The user who defines the server becomes its owner."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:39(para)
msgid "A foreign server typically encapsulates connection information that a foreign-data wrapper uses to access an external data resource. Additional user-specific connection information may be specified by means of user mappings."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:46(para)
msgid "The server name must be unique within the database."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:50(para)
msgid "Creating a server requires <literal>USAGE</literal> privilege on the foreign-data wrapper being used."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:57(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:63(para)
msgid "The name of the foreign server to be created."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:72(para)
msgid "Optional server type."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:81(para)
msgid "Optional server version."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:90(para)
msgid "The name of the foreign-data wrapper that manages the server."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:97(literal)
msgid "OPTIONS ( <placeholder-1/> '<placeholder-2/>' [, ... ] )"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:99(para)
msgid "This clause specifies the options for the server. The options typically define the connection details of the server, but the actual names and values are dependent on the server's foreign-data wrapper."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:111(title)
msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:113(para)
msgid "When using the <application>dblink</application> module (see <xref linkend=\"dblink\"/>), the foreign server name can be used as an argument of the <xref linkend=\"contrib-dblink-connect\"/> function to indicate the connection parameters. See also there for more examples. It is necessary to have the <literal>USAGE</literal> privilege on the foreign server to be able to use it in this way."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:125(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:130(programlisting)
#, no-wrap
msgid "\nCREATE SERVER foo FOREIGN DATA WRAPPER \"default\";\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:127(para)
msgid "Create a server <literal>foo</literal> that uses the built-in foreign-data wrapper <literal>default</literal>: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:138(programlisting)
#, no-wrap
msgid "\nCREATE SERVER myserver FOREIGN DATA WRAPPER pgsql OPTIONS (host 'foo', dbname 'foodb', port '5432');\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_server.xml:135(para)
msgid "Create a server <literal>myserver</literal> that uses the foreign-data wrapper <literal>pgsql</literal>: <placeholder-1/>"
msgstr ""

