#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:24(synopsis)
#, no-wrap
msgid "\nALTER TRIGGER <placeholder-1/> ON <placeholder-2/> RENAME TO <placeholder-3/>\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:32(para)
msgid "<command>ALTER TRIGGER</command> changes properties of an existing trigger. The <literal>RENAME</literal> clause changes the name of the given trigger without otherwise changing the trigger definition."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:39(para)
msgid "You must own the table on which the trigger acts to be allowed to change its properties."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:45(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:51(para)
msgid "The name of an existing trigger to alter."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:60(para)
msgid "The name of the table on which this trigger acts."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:69(para)
msgid "The new name for the trigger."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:78(title)
msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:80(para)
msgid "The ability to temporarily enable or disable a trigger is provided by <xref linkend=\"SQL-ALTERTABLE\"/>, not by <command>ALTER TRIGGER</command>, because <command>ALTER TRIGGER</command> has no convenient way to express the option of enabling or disabling all of a table's triggers at once."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:90(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_trigger.xml:94(programlisting)
#, no-wrap
msgid "\nALTER TRIGGER emp_stamp ON emp RENAME TO emp_track_chgs;\n"
msgstr ""

