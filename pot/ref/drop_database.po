msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_database.xml:53(para)
msgid "Do not throw an error if the database does not exist. A notice is issued in this case."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_database.xml:63(para)
msgid "The name of the database to remove."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_database.xml:72(title)
msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_database.xml:74(para)
msgid "<command>DROP DATABASE</command> cannot be executed inside a transaction block."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_database.xml:79(para)
msgid "This command cannot be executed while connected to the target database. Thus, it might be more convenient to use the program <xref linkend=\"app-dropdb\"/> instead, which is a wrapper around this command."
msgstr ""

