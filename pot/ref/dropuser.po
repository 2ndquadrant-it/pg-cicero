#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:34(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:36(para)
msgid "<application>dropuser</application> removes an existing <productname>PostgreSQL</productname> user. Only superusers and users with the <literal>CREATEROLE</literal> privilege can remove <productname>PostgreSQL</productname> users. (To remove a superuser, you must yourself be a superuser.)"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:44(para)
msgid "<application>dropuser</application> is a wrapper around the <acronym>SQL</acronym> command <xref linkend=\"SQL-DROPROLE\"/>. There is no effective difference between dropping users via this utility and via other methods for accessing the server."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:55(title)
msgid "Options"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:64(para)
msgid "Specifies the name of the <productname>PostgreSQL</productname> user to be removed. You will be prompted for a name if none is specified on the command line."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:72(option)
msgid "-e"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:73(option)
msgid "--echo"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:75(para)
msgid "Echo the commands that <application>dropuser</application> generates and sends to the server."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:83(option)
msgid "-i"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:84(option)
msgid "--interactive"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:86(para)
msgid "Prompt for confirmation before actually removing the user."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:93(option)
msgid "-V"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:94(option)
msgid "--version"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:96(para)
msgid "Print the <application>dropuser</application> version and exit."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:103(option)
msgid "-?"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:104(option)
msgid "--help"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:106(para)
msgid "Show help about <application>dropuser</application> command line arguments, and exit."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:57(para)
msgid "<application>dropuser</application> accepts the following command-line arguments: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:122(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:123(replaceable)
msgid "host"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:122(option)
msgid "-h <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:123(option)
msgid "--host <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:125(para)
msgid "Specifies the host name of the machine on which the server is running. If the value begins with a slash, it is used as the directory for the Unix domain socket."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:135(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:136(replaceable)
msgid "port"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:135(option)
msgid "-p <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:136(option)
msgid "--port <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:138(para)
msgid "Specifies the TCP port or local Unix domain socket file extension on which the server is listening for connections."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:147(option)
msgid "-U <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:148(option)
msgid "--username <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:150(para)
msgid "User name to connect as (not the user name to drop)."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:157(option)
msgid "-w"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:158(option)
msgid "--no-password"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:160(para)
msgid "Never issue a password prompt. If the server requires password authentication and a password is not available by other means such as a <filename>.pgpass</filename> file, the connection attempt will fail. This option can be useful in batch jobs and scripts where no user is present to enter a password."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:172(option)
msgid "-W"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:173(option)
msgid "--password"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:175(para)
msgid "Force <application>dropuser</application> to prompt for a password before connecting to a database."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropuser.xml:180(para)
msgid "This option is never essential, since <application>dropuser</application> will automatically prompt for a password if the server demands password authentication. However, <application>dropuser</application> will waste a connection attempt finding out that the server wants a password. In some cases it is worth typing <option>-W</option> to avoid the extra connection attempt."
msgstr ""

