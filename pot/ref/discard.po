#: /tmp/cicero-YDWB/ref/discard.xml:16(refpurpose) 
msgid "discard session state"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:24(synopsis) 
#, no-wrap
msgid "\nDISCARD { ALL | PLANS | TEMPORARY | TEMP }\n"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:32(para) 
msgid "<command>DISCARD</command> releases internal resources associated with a database session. These resources are normally released at the end of the session."
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:38(para) 
msgid "<command>DISCARD TEMP</command> drops all temporary tables created in the current session. <command>DISCARD PLANS</command> releases all internally cached query plans. <command>DISCARD ALL</command> resets a session to its original state, discarding temporary resources and resetting session-local configuration changes."
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:48(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:53(term) 
msgid "<literal>TEMPORARY</literal> or <literal>TEMP</literal>"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:55(para) 
msgid "Drops all temporary tables created in the current session."
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:62(literal) 
msgid "PLANS"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:64(para) 
msgid "Releases all cached query plans."
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:71(literal) 
msgid "ALL"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:78(programlisting) 
#, no-wrap
msgid "\nSET SESSION AUTHORIZATION DEFAULT;\nRESET ALL;\nDEALLOCATE ALL;\nCLOSE ALL;\nUNLISTEN *;\nSELECT pg_advisory_unlock_all();\nDISCARD PLANS;\nDISCARD TEMP;\n"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:73(para) 
msgid "Releases all temporary resources associated with the current session and resets the session to its initial state. Currently, this has the same effect as executing the following sequence of statements: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:95(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:97(para) 
msgid "<command>DISCARD ALL</command> cannot be executed inside a transaction block."
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:103(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-YDWB/ref/discard.xml:105(para) 
msgid "<command>DISCARD</command> is a <productname>PostgreSQL</productname> extension."
msgstr ""

