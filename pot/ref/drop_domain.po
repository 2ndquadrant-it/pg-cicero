msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:16(refpurpose) 
msgid "remove a domain"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:25(replaceable) /tmp/cicero-pPcb/ref/drop_domain.xml:53(replaceable) 
msgid "name"
