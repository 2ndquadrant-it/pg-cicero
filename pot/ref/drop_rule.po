#: /tmp/cicero-dOlG/ref/drop_rule.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP RULE [ IF EXISTS ] <placeholder-1/> ON <placeholder-2/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:32(para) 
msgid "<command>DROP RULE</command> drops a rewrite rule."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:38(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:43(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:45(para) 
msgid "Do not throw an error if the rule does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:55(para) 
msgid "The name of the rule to drop."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:64(para) 
msgid "The name (optionally schema-qualified) of the table or view that the rule applies to."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:72(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:74(para) 
msgid "Automatically drop objects that depend on the rule."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:81(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:83(para) 
msgid "Refuse to drop the rule if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:93(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:98(programlisting) 
#, no-wrap
msgid "\nDROP RULE newrule ON mytable;\n"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:95(para) 
msgid "To drop the rewrite rule <literal>newrule</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:104(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_rule.xml:106(para) 
msgid "There is no <command>DROP RULE</command> statement in the SQL standard."
msgstr ""

