#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:24(synopsis)
#, no-wrap
msgid "\nALTER TEXT SEARCH DICTIONARY <placeholder-1/> (\n    <placeholder-2/> [ = <placeholder-3/> ] [, ... ]\n)\nALTER TEXT SEARCH DICTIONARY <placeholder-4/> RENAME TO <placeholder-5/>\nALTER TEXT SEARCH DICTIONARY <placeholder-6/> OWNER TO <placeholder-7/>\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:34(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:36(para)
msgid "<command>ALTER TEXT SEARCH DICTIONARY</command> changes the definition of a text search dictionary. You can change the dictionary's template-specific options, or change the dictionary's name or owner."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:42(para)
msgid "You must be the owner of the dictionary to use <command>ALTER TEXT SEARCH DICTIONARY</command>."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:49(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:55(para)
msgid "The name (optionally schema-qualified) of an existing text search dictionary."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:65(para)
msgid "The name of a template-specific option to be set for this dictionary."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:74(para)
msgid "The new value to use for a template-specific option. If the equal sign and value are omitted, then any previous setting for the option is removed from the dictionary, allowing the default to be used."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:86(para)
msgid "The new name of the text search dictionary."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:95(para)
msgid "The new owner of the text search dictionary."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:102(para)
msgid "Template-specific options can appear in any order."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:108(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:110(para)
msgid "The following example command changes the stopword list for a Snowball-based dictionary. Other parameters remain unchanged."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:115(programlisting)
#, no-wrap
msgid "\nALTER TEXT SEARCH DICTIONARY my_dict ( StopWords = newrussian );\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:119(para)
msgid "The following example command changes the language option to dutch, and removes the stopword option entirely."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:124(programlisting)
#, no-wrap
msgid "\nALTER TEXT SEARCH DICTIONARY my_dict ( language = dutch, StopWords );\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:132(programlisting)
#, no-wrap
msgid "\nALTER TEXT SEARCH DICTIONARY my_dict ( dummy );\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:128(para)
msgid "The following example command <quote>updates</quote> the dictionary's definition without actually changing anything. <placeholder-1/> (The reason this works is that the option removal code doesn't complain if there is no such option.) This trick is useful when changing configuration files for the dictionary: the <command>ALTER</command> will force existing database sessions to re-read the configuration files, which otherwise they would never do if they had read them earlier."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:145(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/alter_tsdictionary.xml:147(para)
msgid "There is no <command>ALTER TEXT SEARCH DICTIONARY</command> statement in the SQL standard."
msgstr ""

