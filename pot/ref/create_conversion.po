msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:127(para)
msgid "Use <command>DROP CONVERSION</command> to remove user-defined conversions."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:131(para)
msgid "The privileges required to create a conversion might be changed in a future release."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:138(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:143(programlisting)
#, no-wrap
msgid "\nCREATE CONVERSION myconv FOR 'UTF8' TO 'LATIN1' FROM myfunc;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:140(para)
msgid "To create a conversion from encoding <literal>UTF8</literal> to <literal>LATIN1</literal> using <function>myfunc</function>: <placeholder-1/>"
msgstr ""

