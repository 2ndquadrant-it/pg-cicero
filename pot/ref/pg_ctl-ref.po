msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/pg_ctl-ref.xml:447(para)
msgid "The existence of this file in the data directory is used to help <application>pg_ctl</application> determine if the server is currently running or not."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/pg_ctl-ref.xml:456(filename)
msgid "postmaster.opts"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/pg_ctl-ref.xml:459(para)
