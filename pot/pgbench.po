#: /tmp/cicero-ZMws/pgbench.xml:169(title) 
msgid "<application>pgbench</application> Benchmarking Options"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:178(option) 
msgid "-c"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:178(replaceable) 
msgid "clients"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:180(para) 
msgid "Number of clients simulated, that is, number of concurrent database sessions. Default is 1."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:188(option) 
msgid "-C"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:190(para) 
msgid "Establish a new connection for each transaction, rather than doing it just once per client session. This is useful to measure the connection overhead."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:199(option) 
msgid "-d"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:201(para) 
msgid "Print debugging output."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:208(option) 
msgid "-D"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:208(replaceable) /tmp/cicero-ZMws/pgbench.xml:479(replaceable) /tmp/cicero-ZMws/pgbench.xml:502(replaceable) /tmp/cicero-ZMws/pgbench.xml:549(replaceable) 
msgid "varname"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:208(literal) 
msgid "="
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:208(replaceable) 
msgid "value"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:210(para) 
msgid "Define a variable for use by a custom script (see below). Multiple <literal>-D</literal> options are allowed."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:218(option) 
msgid "-f"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:218(replaceable) 
msgid "filename"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:220(para) 
msgid "Read transaction script from <replaceable>filename</replaceable>. See below for details. <literal>-N</literal>, <literal>-S</literal>, and <literal>-f</literal> are mutually exclusive."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:230(option) 
msgid "-j"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:230(replaceable) 
msgid "threads"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:232(para) 
msgid "Number of worker threads within <application>pgbench</application>. Using more than one thread can be helpful on multi-CPU machines. The number of clients must be a multiple of the number of threads, since each thread is given the same number of client sessions to manage. Default is 1."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:243(option) 
msgid "-l"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:245(para) 
msgid "Write the time taken by each transaction to a log file. See below for details."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:253(option) 
msgid "-M"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:253(replaceable) 
msgid "querymode"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:259(para) 
msgid "<literal>simple</literal>: use simple query protocol."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:262(para) 
msgid "<literal>extended</literal>: use extended query protocol."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:265(para) 
msgid "<literal>prepared</literal>: use extended query protocol with prepared statements."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:255(para) 
msgid "Protocol to use for submitting queries to the server: <placeholder-1/> The default is simple query protocol. (See <xref linkend=\"protocol\"/> for more information.)"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:275(option) 
msgid "-n"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:277(para) 
msgid "Perform no vacuuming before running the test. This option is <emphasis>necessary</emphasis> if you are running a custom test scenario that does not include the standard tables <structname>pgbench_accounts</structname>, <structname>pgbench_branches</structname>, <structname>pgbench_history</structname>, and <structname>pgbench_tellers</structname>."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:289(option) 
msgid "-N"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:291(para) 
msgid "Do not update <structname>pgbench_tellers</structname> and <structname>pgbench_branches</structname>. This will avoid update contention on these tables, but it makes the test case even less like TPC-B."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:301(option) 
msgid "-r"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:303(para) 
msgid "Report the average per-statement latency (execution time from the perspective of the client) of each command after the benchmark finishes. See below for details."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:314(para) 
msgid "Report the specified scale factor in <application>pgbench</application>'s output. With the built-in tests, this is not necessary; the correct scale factor will be detected by counting the number of rows in the <structname>pgbench_branches</structname> table. However, when testing custom benchmarks (<literal>-f</literal> option), the scale factor will be reported as 1 unless this option is used."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:326(option) 
msgid "-S"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:328(para) 
msgid "Perform select-only transactions instead of TPC-B-like test."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:335(option) 
msgid "-t"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:335(replaceable) 
msgid "transactions"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:337(para) 
msgid "Number of transactions each client runs. Default is 10."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:344(option) 
msgid "-T"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:344(replaceable) 
msgid "seconds"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:346(para) 
msgid "Run the test for this many seconds, rather than a fixed number of transactions per client. <literal>-t</literal> and <literal>-T</literal> are mutually exclusive."
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:355(option) 
msgid "-v"
msgstr ""

#: /tmp/cicero-ZMws/pgbench.xml:357(para) 
msgid "Vacuum all four standard tables before running the test. With neither <literal>-n</literal> nor <literal>-v</literal>, pgbench will vacuum the <structname>pgbench_tellers</structname> and <structname>pgbench_branches</structname> tables, and will truncate <structname>pgbench_history</structname>."
msgstr ""

