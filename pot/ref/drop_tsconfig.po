msgid "DROP TEXT SEARCH CONFIGURATION"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:16(refpurpose) 
msgid "remove a text search configuration"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:25(replaceable) /tmp/cicero-cCcM/ref/drop_tsconfig.xml:55(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP TEXT SEARCH CONFIGURATION [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:32(para) 
msgid "<command>DROP TEXT SEARCH CONFIGURATION</command> drops an existing text search configuration. To execute this command you must be the owner of the configuration."
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:47(para) 
msgid "Do not throw an error if the text search configuration does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:57(para) 
msgid "The name (optionally schema-qualified) of an existing text search configuration."
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:65(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:67(para) 
msgid "Automatically drop objects that depend on the text search configuration."
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:74(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:76(para) 
msgid "Refuse to drop the text search configuration if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:86(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:91(programlisting) 
#, no-wrap
msgid "\nDROP TEXT SEARCH CONFIGURATION my_english;\n"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:88(para) 
msgid "Remove the text search configuration <literal>my_english</literal>: <placeholder-1/> This command will not succeed if there are any existing indexes that reference the configuration in <function>to_tsvector</function> calls. Add <literal>CASCADE</literal> to drop such indexes along with the text search configuration."
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:103(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-cCcM/ref/drop_tsconfig.xml:105(para) 
msgid "There is no <command>DROP TEXT SEARCH CONFIGURATION</command> statement in the SQL standard."
msgstr ""

