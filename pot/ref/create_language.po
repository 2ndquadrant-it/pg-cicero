msgid "\nCREATE LANGUAGE plperl;\n"
msgstr ""

#: /tmp/cicero-YDWB/ref/create_language.xml:292(para) 
msgid "The preferred way of creating any of the standard procedural languages is just: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-YDWB/ref/create_language.xml:303(programlisting) 
#, no-wrap
msgid "\nCREATE FUNCTION plsample_call_handler() RETURNS language_handler\n    AS '$libdir/plsample'\n    LANGUAGE C;\nCREATE LANGUAGE plsample\n    HANDLER plsample_call_handler;\n"
msgstr ""

