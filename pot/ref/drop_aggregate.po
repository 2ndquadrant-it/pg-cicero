msgid "DROP AGGREGATE"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:17(refpurpose) 
msgid "remove an aggregate function"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:26(replaceable) /tmp/cicero-Bcej/ref/drop_aggregate.xml:56(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:26(replaceable) /tmp/cicero-Bcej/ref/drop_aggregate.xml:65(replaceable) 
msgid "type"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP AGGREGATE [ IF EXISTS ] <placeholder-1/> ( <placeholder-2/> [ , ... ] ) [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:33(para) 
msgid "<command>DROP AGGREGATE</command> removes an existing aggregate function. To execute this command the current user must be the owner of the aggregate function."
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:46(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:48(para) 
msgid "Do not throw an error if the aggregate does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:58(para) 
msgid "The name (optionally schema-qualified) of an existing aggregate function."
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:67(para) 
msgid "An input data type on which the aggregate function operates. To reference a zero-argument aggregate function, write <literal>*</literal> in place of the list of input data types."
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:76(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:78(para) 
msgid "Automatically drop objects that depend on the aggregate function."
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:85(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:87(para) 
msgid "Refuse to drop the aggregate function if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:97(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:102(programlisting) 
#, no-wrap
msgid "\nDROP AGGREGATE myavg(integer);\n"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:99(para) 
msgid "To remove the aggregate function <literal>myavg</literal> for type <type>integer</type>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:108(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-Bcej/ref/drop_aggregate.xml:110(para) 
msgid "There is no <command>DROP AGGREGATE</command> statement in the SQL standard."
msgstr ""

