#, no-wrap
msgid "\nALTER TABLESPACE <placeholder-1/> RENAME TO <placeholder-2/>\nALTER TABLESPACE <placeholder-3/> OWNER TO <placeholder-4/>\nALTER TABLESPACE <placeholder-5/> SET ( <placeholder-6/> = <placeholder-7/> [, ... ] )\nALTER TABLESPACE <placeholder-8/> RESET ( <placeholder-9/> [, ... ] )\n"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:33(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:35(para) 
msgid "<command>ALTER TABLESPACE</command> changes the definition of a tablespace."
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:40(para) 
msgid "You must own the tablespace to use <command>ALTER TABLESPACE</command>. To alter the owner, you must also be a direct or indirect member of the new owning role. (Note that superusers have these privileges automatically.)"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:49(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:55(para) 
msgid "The name of an existing tablespace."
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:64(para) 
msgid "The new name of the tablespace. The new name cannot begin with <literal>pg_</literal>, as such names are reserved for system tablespaces."
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:75(para) 
msgid "The new owner of the tablespace."
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:82(replaceable) 
msgid "tablespace_parameter"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:84(para) 
msgid "A tablespace parameter to be set or reset. Currently, the only available parameters are <varname>seq_page_cost</varname> and <varname>random_page_cost</varname>. Setting either value for a particular tablespace will override the planner's usual estimate of the cost of reading pages from tables in that tablespace, as established by the configuration parameters of the same name (see <xref linkend=\"guc-seq-page-cost\"/>, <xref linkend=\"guc-random-page-cost\"/>). This may be useful if one tablespace is located on a disk which is faster or slower than the remainder of the I/O subsystem."
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:102(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:106(programlisting) 
#, no-wrap
msgid "\nALTER TABLESPACE index_space RENAME TO fast_raid;\n"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:104(para) 
msgid "Rename tablespace <literal>index_space</literal> to <literal>fast_raid</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:113(programlisting) 
#, no-wrap
msgid "\nALTER TABLESPACE index_space OWNER TO mary;\n"
msgstr ""

#: /tmp/cicero-fQdk/ref/alter_tablespace.xml:111(para) 
msgid "Change the owner of tablespace <literal>index_space</literal>: <placeholder-1/>"
msgstr ""

