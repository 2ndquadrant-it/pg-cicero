msgid "Environment"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:221(envar) 
msgid "PGDATABASE"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:222(envar) 
msgid "PGHOST"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:223(envar) 
msgid "PGPORT"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:224(envar) 
msgid "PGUSER"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:227(para) 
msgid "Default connection parameters"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:234(para) 
msgid "This utility, like most other <productname>PostgreSQL</productname> utilities, also uses the environment variables supported by <application>libpq</application> (see <xref linkend=\"libpq-envars\"/>)."
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:244(title) 
msgid "Diagnostics"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:246(para) 
msgid "Most error messages are self-explanatory. If not, run <application>createlang</application> with the <option>--echo</option> option and see the respective <acronym>SQL</acronym> command for details. Also, any default connection settings and environment variables used by the <application>libpq</application> front-end library will apply."
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:258(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:260(para) 
msgid "Use <xref linkend=\"app-droplang\"/> to remove a language."
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:267(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-krfh/ref/createlang.xml:269(para) 
msgid "To install the language <literal>pltcl</literal> into the database <literal>template1</literal>: <screen>\n<prompt>$ </prompt><userinput>createlang pltcl template1</userinput>\n</screen> Note that installing the language into <literal>template1</literal> will cause it to be automatically installed into subsequently-created databases as well."
msgstr ""

