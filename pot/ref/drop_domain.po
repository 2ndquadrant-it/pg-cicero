#: /tmp/cicero-pPcb/ref/drop_domain.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP DOMAIN [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:32(para) 
msgid "<command>DROP DOMAIN</command> removes a domain. Only the owner of a domain can remove it."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:39(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:43(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:45(para) 
msgid "Do not throw an error if the domain does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:55(para) 
msgid "The name (optionally schema-qualified) of an existing domain."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:62(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:64(para) 
msgid "Automatically drop objects that depend on the domain (such as table columns)."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:72(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:74(para) 
msgid "Refuse to drop the domain if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:84(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:89(programlisting) 
#, no-wrap
msgid "\nDROP DOMAIN box;\n"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:86(para) 
msgid "To remove the domain <type>box</type>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:95(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-pPcb/ref/drop_domain.xml:97(para) 
msgid "This command conforms to the SQL standard, except for the <literal>IF EXISTS</literal> option, which is a <productname>PostgreSQL</productname> extension."
msgstr ""

