msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:16(refpurpose)
msgid "remove a text search parser"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:25(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:54(replaceable)
msgid "name"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:24(synopsis)
#, no-wrap
msgid "\nDROP TEXT SEARCH PARSER [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:32(para)
msgid "<command>DROP TEXT SEARCH PARSER</command> drops an existing text search parser. You must be a superuser to use this command."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:39(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:44(literal)
msgid "IF EXISTS"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:46(para)
msgid "Do not throw an error if the text search parser does not exist. A notice is issued in this case."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:56(para)
msgid "The name (optionally schema-qualified) of an existing text search parser."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:63(literal)
msgid "CASCADE"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:65(para)
msgid "Automatically drop objects that depend on the text search parser."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:72(literal)
msgid "RESTRICT"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:74(para)
msgid "Refuse to drop the text search parser if any objects depend on it. This is the default."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:84(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:89(programlisting)
#, no-wrap
msgid "\nDROP TEXT SEARCH PARSER my_parser;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:86(para)
msgid "Remove the text search parser <literal>my_parser</literal>: <placeholder-1/> This command will not succeed if there are any existing text search configurations that use the parser. Add <literal>CASCADE</literal> to drop such configurations along with the parser."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:100(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tsparser.xml:102(para)
msgid "There is no <command>DROP TEXT SEARCH PARSER</command> statement in the SQL standard."
msgstr ""

