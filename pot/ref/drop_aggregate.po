#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:24(synopsis)
#, no-wrap
msgid "\nDROP AGGREGATE [ IF EXISTS ] <placeholder-1/> ( <placeholder-2/> [ , ... ] ) [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:32(para)
msgid "<command>DROP AGGREGATE</command> removes an existing aggregate function. To execute this command the current user must be the owner of the aggregate function."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:40(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:45(literal)
msgid "IF EXISTS"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:47(para)
msgid "Do not throw an error if the aggregate does not exist. A notice is issued in this case."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:57(para)
msgid "The name (optionally schema-qualified) of an existing aggregate function."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:66(para)
msgid "An input data type on which the aggregate function operates. To reference a zero-argument aggregate function, write <literal>*</literal> in place of the list of input data types."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:75(literal)
msgid "CASCADE"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:77(para)
msgid "Automatically drop objects that depend on the aggregate function."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:84(literal)
msgid "RESTRICT"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:86(para)
msgid "Refuse to drop the aggregate function if any objects depend on it. This is the default."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:96(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:101(programlisting)
#, no-wrap
msgid "\nDROP AGGREGATE myavg(integer);\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_aggregate.xml:98(para)
msgid "To remove the aggregate function <literal>myavg</literal> for type <type>integer</type>: <placeholder-1/>"
msgstr ""

