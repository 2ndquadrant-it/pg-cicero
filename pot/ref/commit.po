#: /tmp/cicero-dQiJ/ref/commit.xml:17(refpurpose) 
msgid "commit the current transaction"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:25(synopsis) 
#, no-wrap
msgid "\nCOMMIT [ WORK | TRANSACTION ]\n"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:33(para) 
msgid "<command>COMMIT</command> commits the current transaction. All changes made by the transaction become visible to others and are guaranteed to be durable if a crash occurs."
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:45(literal) 
msgid "WORK"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:46(literal) 
msgid "TRANSACTION"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:48(para) 
msgid "Optional key words. They have no effect."
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:57(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:59(para) 
msgid "Use <xref linkend=\"sql-rollback\"/> to abort a transaction."
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:64(para) 
msgid "Issuing <command>COMMIT</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:71(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:75(programlisting) 
#, no-wrap
msgid "\nCOMMIT;\n"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:73(para) 
msgid "To commit the current transaction and make all changes permanent: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:81(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-dQiJ/ref/commit.xml:83(para) 
msgid "The SQL standard only specifies the two forms <literal>COMMIT</literal> and <literal>COMMIT WORK</literal>. Otherwise, this command is fully conforming."
msgstr ""

