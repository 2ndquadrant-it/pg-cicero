msgid "ROLLBACK"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:17(refpurpose) 
msgid "abort the current transaction"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:25(synopsis) 
#, no-wrap
msgid "\nROLLBACK [ WORK | TRANSACTION ]\n"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:33(para) 
msgid "<command>ROLLBACK</command> rolls back the current transaction and causes all the updates made by the transaction to be discarded."
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:44(literal) 
msgid "WORK"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:45(literal) 
msgid "TRANSACTION"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:47(para) 
msgid "Optional key words. They have no effect."
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:56(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:58(para) 
msgid "Use <xref linkend=\"sql-commit\"/> to successfully terminate a transaction."
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:63(para) 
msgid "Issuing <command>ROLLBACK</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:70(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:74(programlisting) 
#, no-wrap
msgid "\nROLLBACK;\n"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:72(para) 
msgid "To abort all changes: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:80(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-NXtK/ref/rollback.xml:82(para) 
msgid "The SQL standard only specifies the two forms <literal>ROLLBACK</literal> and <literal>ROLLBACK WORK</literal>. Otherwise, this command is fully conforming."
msgstr ""

