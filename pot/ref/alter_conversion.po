#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:24(synopsis) 
#, no-wrap
msgid "\nALTER CONVERSION <placeholder-1/> RENAME TO <placeholder-2/>\nALTER CONVERSION <placeholder-3/> OWNER TO <placeholder-4/>\nALTER CONVERSION <placeholder-5/> SET SCHEMA <placeholder-6/>\n"
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:32(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:34(para) 
msgid "<command>ALTER CONVERSION</command> changes the definition of a conversion."
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:39(para) 
msgid "You must own the conversion to use <command>ALTER CONVERSION</command>. To alter the owner, you must also be a direct or indirect member of the new owning role, and that role must have <literal>CREATE</literal> privilege on the conversion's schema. (These restrictions enforce that altering the owner doesn't do anything you couldn't do by dropping and recreating the conversion. However, a superuser can alter ownership of any conversion anyway.)"
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:51(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:57(para) 
msgid "The name (optionally schema-qualified) of an existing conversion."
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:66(para) 
msgid "The new name of the conversion."
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:75(para) 
msgid "The new owner of the conversion."
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:84(para) 
msgid "The new schema for the conversion."
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:93(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:98(programlisting) 
#, no-wrap
msgid "\nALTER CONVERSION iso_8859_1_to_utf8 RENAME TO latin1_to_unicode;\n"
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:95(para) 
msgid "To rename the conversion <literal>iso_8859_1_to_utf8</literal> to <literal>latin1_to_unicode</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:106(programlisting) 
#, no-wrap
msgid "\nALTER CONVERSION iso_8859_1_to_utf8 OWNER TO joe;\n"
msgstr ""

#: /tmp/cicero-ZqRK/ref/alter_conversion.xml:103(para) 
msgid "To change the owner of the conversion <literal>iso_8859_1_to_utf8</literal> to <literal>joe</literal>: <placeholder-1/>"
msgstr ""

