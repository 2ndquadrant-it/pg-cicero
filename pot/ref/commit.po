#: /tmp/cicero-fBJf/ref/commit.xml:16(refpurpose) 
msgid "commit the current transaction"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:24(synopsis) 
#, no-wrap
msgid "\nCOMMIT [ WORK | TRANSACTION ]\n"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:32(para) 
msgid "<command>COMMIT</command> commits the current transaction. All changes made by the transaction become visible to others and are guaranteed to be durable if a crash occurs."
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:44(literal) 
msgid "WORK"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:45(literal) 
msgid "TRANSACTION"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:47(para) 
msgid "Optional key words. They have no effect."
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:56(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:58(para) 
msgid "Use <xref linkend=\"sql-rollback\"/> to abort a transaction."
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:63(para) 
msgid "Issuing <command>COMMIT</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:70(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:74(programlisting) 
#, no-wrap
msgid "\nCOMMIT;\n"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:72(para) 
msgid "To commit the current transaction and make all changes permanent: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:80(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-fBJf/ref/commit.xml:82(para) 
msgid "The SQL standard only specifies the two forms <literal>COMMIT</literal> and <literal>COMMIT WORK</literal>. Otherwise, this command is fully conforming."
msgstr ""

