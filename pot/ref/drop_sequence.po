#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:16(refpurpose) 
msgid "remove a sequence"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:25(replaceable) /tmp/cicero-cjWZ/ref/drop_sequence.xml:53(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP SEQUENCE [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:32(para) 
msgid "<command>DROP SEQUENCE</command> removes sequence number generators. A sequence can only be dropped by its owner or a superuser."
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:39(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:43(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:45(para) 
msgid "Do not throw an error if the sequence does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:55(para) 
msgid "The name (optionally schema-qualified) of a sequence."
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:62(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:64(para) 
msgid "Automatically drop objects that depend on the sequence."
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:71(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:73(para) 
msgid "Refuse to drop the sequence if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:83(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:88(programlisting) 
#, no-wrap
msgid "\nDROP SEQUENCE serial;\n"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:85(para) 
msgid "To remove the sequence <literal>serial</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:94(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-cjWZ/ref/drop_sequence.xml:96(para) 
msgid "<command>DROP SEQUENCE</command> conforms to the <acronym>SQL</acronym> standard, except that the standard only allows one sequence to be dropped per command, and apart from the <literal>IF EXISTS</literal> option, which is a <productname>PostgreSQL</productname> extension."
msgstr ""

