msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:22(synopsis) 
#, no-wrap
msgid "\nDROP CONVERSION [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:28(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:30(para) 
msgid "<command>DROP CONVERSION</command> removes a previously defined conversion. To be able to drop a conversion, you must own the conversion."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:37(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:41(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:43(para) 
msgid "Do not throw an error if the conversion does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:54(para) 
msgid "The name of the conversion. The conversion name can be schema-qualified."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:62(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:63(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:66(para) 
msgid "These key words do not have any effect, since there are no dependencies on conversions."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:76(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:80(programlisting) 
#, no-wrap
msgid "\nDROP CONVERSION myname;\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:78(para) 
msgid "To drop the conversion named <literal>myname</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:86(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_conversion.xml:88(para) 
msgid "There is no <command>DROP CONVERSION</command> statement in the SQL standard, but a <command>DROP TRANSLATION</command> statement that goes along with the <command>CREATE TRANSLATION</command> statement that is similar to the <command>CREATE CONVERSION</command> statement in PostgreSQL."
msgstr ""

