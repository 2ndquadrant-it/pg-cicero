msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:17(refpurpose) 
msgid "remove a domain"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:26(replaceable) /tmp/cicero-lkRf/ref/drop_domain.xml:54(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP DOMAIN [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:33(para) 
msgid "<command>DROP DOMAIN</command> removes a domain. Only the owner of a domain can remove it."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:44(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:46(para) 
msgid "Do not throw an error if the domain does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:56(para) 
msgid "The name (optionally schema-qualified) of an existing domain."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:63(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:65(para) 
msgid "Automatically drop objects that depend on the domain (such as table columns)."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:73(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:75(para) 
msgid "Refuse to drop the domain if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:85(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:90(programlisting) 
#, no-wrap
msgid "\nDROP DOMAIN box;\n"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:87(para) 
msgid "To remove the domain <type>box</type>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:96(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_domain.xml:98(para) 
msgid "This command conforms to the SQL standard, except for the <literal>IF EXISTS</literal> option, which is a <productname>PostgreSQL</productname> extension."
msgstr ""

