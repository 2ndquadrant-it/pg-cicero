msgid "DROP TEXT SEARCH PARSER"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:17(refpurpose) 
msgid "remove a text search parser"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:26(replaceable) /tmp/cicero-lyhk/ref/drop_tsparser.xml:55(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP TEXT SEARCH PARSER [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:33(para) 
msgid "<command>DROP TEXT SEARCH PARSER</command> drops an existing text search parser. You must be a superuser to use this command."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:47(para) 
msgid "Do not throw an error if the text search parser does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:57(para) 
msgid "The name (optionally schema-qualified) of an existing text search parser."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:64(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:66(para) 
msgid "Automatically drop objects that depend on the text search parser."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:73(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:75(para) 
msgid "Refuse to drop the text search parser if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:85(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:90(programlisting) 
#, no-wrap
msgid "\nDROP TEXT SEARCH PARSER my_parser;\n"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:87(para) 
msgid "Remove the text search parser <literal>my_parser</literal>: <placeholder-1/> This command will not succeed if there are any existing text search configurations that use the parser. Add <literal>CASCADE</literal> to drop such configurations along with the parser."
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:101(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-lyhk/ref/drop_tsparser.xml:103(para) 
msgid "There is no <command>DROP TEXT SEARCH PARSER</command> statement in the SQL standard."
msgstr ""

