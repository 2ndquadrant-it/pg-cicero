#, no-wrap
msgid "\nALTER OPERATOR <placeholder-1/> ( { <placeholder-2/> | NONE } , { <placeholder-3/> | NONE } ) OWNER TO <placeholder-4/>\nALTER OPERATOR <placeholder-5/> ( { <placeholder-6/> | NONE } , { <placeholder-7/> | NONE } ) SET SCHEMA <placeholder-8/>\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:32(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:34(para) 
msgid "<command>ALTER OPERATOR</command> changes the definition of an operator. The only currently available functionality is to change the owner of the operator."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:40(para) 
msgid "You must own the operator to use <command>ALTER OPERATOR</command>. To alter the owner, you must also be a direct or indirect member of the new owning role, and that role must have <literal>CREATE</literal> privilege on the operator's schema. (These restrictions enforce that altering the owner doesn't do anything you couldn't do by dropping and recreating the operator. However, a superuser can alter ownership of any operator anyway.)"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:51(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:57(para) 
msgid "The name (optionally schema-qualified) of an existing operator."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:66(para) 
msgid "The data type of the operator's left operand; write <literal>NONE</literal> if the operator has no left operand."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:76(para) 
msgid "The data type of the operator's right operand; write <literal>NONE</literal> if the operator has no right operand."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:86(para) 
msgid "The new owner of the operator."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:95(para) 
msgid "The new schema for the operator."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:104(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:108(programlisting) 
#, no-wrap
msgid "\nALTER OPERATOR @@ (text, text) OWNER TO joe;\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_operator.xml:106(para) 
msgid "Change the owner of a custom operator <literal>a @@ b</literal> for type <type>text</type>: <placeholder-1/>"
msgstr ""

