msgid "DROP INDEX"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:16(refpurpose) 
msgid "remove an index"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:25(replaceable) /tmp/cicero-ZqRK/ref/drop_index.xml:54(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP INDEX [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:32(para) 
msgid "<command>DROP INDEX</command> drops an existing index from the database system. To execute this command you must be the owner of the index."
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:44(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:46(para) 
msgid "Do not throw an error if the index does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:56(para) 
msgid "The name (optionally schema-qualified) of an index to remove."
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:63(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:65(para) 
msgid "Automatically drop objects that depend on the index."
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:72(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:74(para) 
msgid "Refuse to drop the index if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:84(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:89(programlisting) 
#, no-wrap
msgid "\nDROP INDEX title_idx;\n"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:86(para) 
msgid "This command will remove the index <literal>title_idx</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:95(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-ZqRK/ref/drop_index.xml:97(para) 
msgid "<command>DROP INDEX</command> is a <productname>PostgreSQL</productname> language extension. There are no provisions for indexes in the SQL standard."
msgstr ""

