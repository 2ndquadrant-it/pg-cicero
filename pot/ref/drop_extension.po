msgid "DROP EXTENSION"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:16(refpurpose) 
msgid "remove an extension"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:25(replaceable) /tmp/cicero-FQJK/ref/drop_extension.xml:58(replaceable) 
msgid "extension_name"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP EXTENSION [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:32(para) 
msgid "<command>DROP EXTENSION</command> removes extensions from the database. Dropping an extension causes its component objects to be dropped as well."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:37(para) 
msgid "You must own the extension to use <command>DROP EXTENSION</command>."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:43(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:48(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:50(para) 
msgid "Do not throw an error if the extension does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:60(para) 
msgid "The name of an installed extension."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:67(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:69(para) 
msgid "Automatically drop objects that depend on the extension."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:76(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:78(para) 
msgid "Refuse to drop the extension if any objects depend on it (other than its own member objects and other extensions listed in the same <command>DROP</command> command). This is the default."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:89(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:94(programlisting) 
#, no-wrap
msgid "\nDROP EXTENSION hstore;\n"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:91(para) 
msgid "To remove the extension <literal>hstore</literal> from the current database: <placeholder-1/> This command will fail if any of <literal>hstore</literal>'s objects are in use in the database, for example if any tables have columns of the <type>hstore</type> type. Add the <literal>CASCADE</literal> option to forcibly remove those dependent objects as well."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:105(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_extension.xml:107(para) 
msgid "<command>DROP EXTENSION</command> is a <productname>PostgreSQL</productname> extension."
msgstr ""

