#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:24(synopsis)
#, no-wrap
msgid "\nDROP OPERATOR [ IF EXISTS ] <placeholder-1/> ( { <placeholder-2/> | NONE } , { <placeholder-3/> | NONE } ) [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:32(para)
msgid "<command>DROP OPERATOR</command> drops an existing operator from the database system. To execute this command you must be the owner of the operator."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:40(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:45(literal)
msgid "IF EXISTS"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:47(para)
msgid "Do not throw an error if the operator does not exist. A notice is issued in this case."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:57(para)
msgid "The name (optionally schema-qualified) of an existing operator."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:66(para)
msgid "The data type of the operator's left operand; write <literal>NONE</literal> if the operator has no left operand."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:76(para)
msgid "The data type of the operator's right operand; write <literal>NONE</literal> if the operator has no right operand."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:84(literal)
msgid "CASCADE"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:86(para)
msgid "Automatically drop objects that depend on the operator."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:93(literal)
msgid "RESTRICT"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:95(para)
msgid "Refuse to drop the operator if any objects depend on it. This is the default."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:105(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:109(programlisting)
#, no-wrap
msgid "\nDROP OPERATOR ^ (integer, integer);\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:107(para)
msgid "Remove the power operator <literal>a^b</literal> for type <type>integer</type>: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:117(programlisting)
#, no-wrap
msgid "\nDROP OPERATOR ~ (none, bit);\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:114(para)
msgid "Remove the left unary bitwise complement operator <literal>~b</literal> for type <type>bit</type>: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:125(programlisting)
#, no-wrap
msgid "\nDROP OPERATOR ! (bigint, none);\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_operator.xml:122(para)
msgid "Remove the right unary factorial operator <literal>x!</literal> for type <type>bigint</type>: <placeholder-1/>"
msgstr ""

