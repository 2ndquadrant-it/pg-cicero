msgid "DROP OPERATOR CLASS"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:16(refpurpose) 
msgid "remove an operator class"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:25(replaceable) /tmp/cicero-GNkR/ref/drop_opclass.xml:61(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:25(replaceable) /tmp/cicero-GNkR/ref/drop_opclass.xml:70(replaceable) 
msgid "index_method"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP OPERATOR CLASS [ IF EXISTS ] <placeholder-1/> USING <placeholder-2/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:32(para) 
msgid "<command>DROP OPERATOR CLASS</command> drops an existing operator class. To execute this command you must be the owner of the operator class."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:37(para) 
msgid "<command>DROP OPERATOR CLASS</command> does not drop any of the operators or functions referenced by the class. If there are any indexes depending on the operator class, you will need to specify <literal>CASCADE</literal> for the drop to complete."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:46(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:51(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:53(para) 
msgid "Do not throw an error if the operator class does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:63(para) 
msgid "The name (optionally schema-qualified) of an existing operator class."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:72(para) 
msgid "The name of the index access method the operator class is for."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:79(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:81(para) 
msgid "Automatically drop objects that depend on the operator class."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:88(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:90(para) 
msgid "Refuse to drop the operator class if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:100(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:102(para) 
msgid "<command>DROP OPERATOR CLASS</command> will not drop the operator family containing the class, even if there is nothing else left in the family (in particular, in the case where the family was implicitly created by <command>CREATE OPERATOR CLASS</command>). An empty operator family is harmless, but for the sake of tidiness you might wish to remove the family with <command>DROP OPERATOR FAMILY</command>; or perhaps better, use <command>DROP OPERATOR FAMILY</command> in the first place."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:114(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:119(programlisting) 
#, no-wrap
msgid "\nDROP OPERATOR CLASS widget_ops USING btree;\n"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:116(para) 
msgid "Remove the B-tree operator class <literal>widget_ops</literal>: <placeholder-1/> This command will not succeed if there are any existing indexes that use the operator class. Add <literal>CASCADE</literal> to drop such indexes along with the operator class."
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:130(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-GNkR/ref/drop_opclass.xml:132(para) 
msgid "There is no <command>DROP OPERATOR CLASS</command> statement in the SQL standard."
msgstr ""

