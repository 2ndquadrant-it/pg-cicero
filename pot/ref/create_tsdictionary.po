#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:24(synopsis)
#, no-wrap
msgid "\nCREATE TEXT SEARCH DICTIONARY <placeholder-1/> (\n    TEMPLATE = <placeholder-2/>\n    [, <placeholder-3/> = <placeholder-4/> [, ... ]]\n)\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:33(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:35(para)
msgid "<command>CREATE TEXT SEARCH DICTIONARY</command> creates a new text search dictionary. A text search dictionary specifies a way of recognizing interesting or uninteresting words for searching. A dictionary depends on a text search template, which specifies the functions that actually perform the work. Typically the dictionary provides some options that control the detailed behavior of the template's functions."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:44(para)
msgid "If a schema name is given then the text search dictionary is created in the specified schema. Otherwise it is created in the current schema."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:49(para)
msgid "The user who defines a text search dictionary becomes its owner."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:53(para)
msgid "Refer to <xref linkend=\"textsearch\"/> for further information."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:59(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:65(para)
msgid "The name of the text search dictionary to be created. The name can be schema-qualified."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:75(para)
msgid "The name of the text search template that will define the basic behavior of this dictionary."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:85(para)
msgid "The name of a template-specific option to be set for this dictionary."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:94(para)
msgid "The value to use for a template-specific option. If the value is not a simple identifier or number, it must be quoted (but you can always quote it, if you wish)."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:103(para)
msgid "The options can appear in any order."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:109(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:111(para)
msgid "The following example command creates a Snowball-based dictionary with a nonstandard list of stop words."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:116(programlisting)
#, no-wrap
msgid "\nCREATE TEXT SEARCH DICTIONARY my_russian (\n    template = snowball,\n    language = russian,\n    stopwords = myrussian\n);\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:126(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_tsdictionary.xml:128(para)
msgid "There is no <command>CREATE TEXT SEARCH DICTIONARY</command> statement in the SQL standard."
msgstr ""

