msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:16(refpurpose) 
msgid "remove a user mapping for a foreign server"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:25(replaceable) /tmp/cicero-JkEb/ref/drop_user_mapping.xml:60(replaceable) 
msgid "user_name"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:25(replaceable) /tmp/cicero-JkEb/ref/drop_user_mapping.xml:72(replaceable) 
msgid "server_name"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP USER MAPPING [ IF EXISTS ] FOR { <placeholder-1/> | USER | CURRENT_USER | PUBLIC } SERVER <placeholder-2/>\n"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:32(para) 
msgid "<command>DROP USER MAPPING</command> removes an existing user mapping from foreign server."
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:37(para) 
msgid "The owner of a foreign server can drop user mappings for that server for any user. Also, a user can drop a user mapping for his own user name if <literal>USAGE</literal> privilege on the server has been granted to the user."
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:46(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:50(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:52(para) 
msgid "Do not throw an error if the user mapping does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:62(para) 
msgid "User name of the mapping. <literal>CURRENT_USER</literal> and <literal>USER</literal> match the name of the current user. <literal>PUBLIC</literal> is used to match all present and future user names in the system."
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:74(para) 
msgid "Server name of the user mapping."
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:83(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:87(programlisting) 
#, no-wrap
msgid "\nDROP USER MAPPING IF EXISTS FOR bob SERVER foo;\n"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:85(para) 
msgid "Drop a user mapping <literal>bob</literal>, server <literal>foo</literal> if it exists: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:93(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-JkEb/ref/drop_user_mapping.xml:95(para) 
msgid "<command>DROP USER MAPPING</command> conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal> clause is a <productname>PostgreSQL</productname> extension."
msgstr ""

