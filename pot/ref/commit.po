#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:11(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:16(refpurpose)
msgid "commit the current transaction"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:24(synopsis)
#, no-wrap
msgid "\nCOMMIT [ WORK | TRANSACTION ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:32(para)
msgid "<command>COMMIT</command> commits the current transaction. All changes made by the transaction become visible to others and are guaranteed to be durable if a crash occurs."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:40(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:44(literal)
msgid "WORK"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:45(literal)
msgid "TRANSACTION"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:47(para)
msgid "Optional key words. They have no effect."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:56(title)
msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:58(para)
msgid "Use <xref linkend=\"SQL-ROLLBACK\"/> to abort a transaction."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:63(para)
msgid "Issuing <command>COMMIT</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:70(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:74(programlisting)
#, no-wrap
msgid "\nCOMMIT;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:72(para)
msgid "To commit the current transaction and make all changes permanent: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:81(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/commit.xml:83(para)
msgid "The SQL standard only specifies the two forms <literal>COMMIT</literal> and <literal>COMMIT WORK</literal>. Otherwise, this command is fully conforming."
msgstr ""

