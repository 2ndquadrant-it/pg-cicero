#: /tmp/cicero-tGKz/ref/drop_server.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:16(refpurpose) 
msgid "remove a foreign server descriptor"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:25(replaceable) /tmp/cicero-tGKz/ref/drop_server.xml:54(replaceable) 
msgid "server_name"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP SERVER [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:32(para) 
msgid "<command>DROP SERVER</command> removes an existing foreign server descriptor. To execute this command, the current user must be the owner of the server."
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:44(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:46(para) 
msgid "Do not throw an error if the server does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:56(para) 
msgid "The name of an existing server."
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:63(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:65(para) 
msgid "Automatically drop objects that depend on the server (such as user mappings)."
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:73(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:75(para) 
msgid "Refuse to drop the server if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:85(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:89(programlisting) 
#, no-wrap
msgid "\nDROP SERVER IF EXISTS foo;\n"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:87(para) 
msgid "Drop a server <literal>foo</literal> if it exists: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:95(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-tGKz/ref/drop_server.xml:97(para) 
msgid "<command>DROP SERVER</command> conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal> clause is a <productname>PostgreSQL</productname> extension."
msgstr ""

