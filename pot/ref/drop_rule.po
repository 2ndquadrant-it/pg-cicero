#: /tmp/cicero-hapq/ref/drop_rule.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP RULE [ IF EXISTS ] <placeholder-1/> ON <placeholder-2/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:33(para) 
msgid "<command>DROP RULE</command> drops a rewrite rule."
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:39(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:44(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:46(para) 
msgid "Do not throw an error if the rule does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:56(para) 
msgid "The name of the rule to drop."
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:65(para) 
msgid "The name (optionally schema-qualified) of the table or view that the rule applies to."
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:73(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:75(para) 
msgid "Automatically drop objects that depend on the rule."
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:82(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:84(para) 
msgid "Refuse to drop the rule if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:94(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:99(programlisting) 
#, no-wrap
msgid "\nDROP RULE newrule ON mytable;\n"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:96(para) 
msgid "To drop the rewrite rule <literal>newrule</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:105(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-hapq/ref/drop_rule.xml:107(para) 
msgid "There is no <command>DROP RULE</command> statement in the SQL standard."
msgstr ""

