msgid "DROP TEXT SEARCH CONFIGURATION"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:17(refpurpose) 
msgid "remove a text search configuration"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:26(replaceable) /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:56(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP TEXT SEARCH CONFIGURATION [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:33(para) 
msgid "<command>DROP TEXT SEARCH CONFIGURATION</command> drops an existing text search configuration. To execute this command you must be the owner of the configuration."
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:46(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:48(para) 
msgid "Do not throw an error if the text search configuration does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:58(para) 
msgid "The name (optionally schema-qualified) of an existing text search configuration."
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:66(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:68(para) 
msgid "Automatically drop objects that depend on the text search configuration."
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:75(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:77(para) 
msgid "Refuse to drop the text search configuration if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:87(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:92(programlisting) 
#, no-wrap
msgid "\nDROP TEXT SEARCH CONFIGURATION my_english;\n"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:89(para) 
msgid "Remove the text search configuration <literal>my_english</literal>: <placeholder-1/> This command will not succeed if there are any existing indexes that reference the configuration in <function>to_tsvector</function> calls. Add <literal>CASCADE</literal> to drop such indexes along with the text search configuration."
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:104(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-dQiJ/ref/drop_tsconfig.xml:106(para) 
msgid "There is no <command>DROP TEXT SEARCH CONFIGURATION</command> statement in the SQL standard."
msgstr ""

