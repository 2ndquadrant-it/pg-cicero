msgid "DROP INDEX"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:17(refpurpose) 
msgid "remove an index"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:26(replaceable) /tmp/cicero-QnGh/ref/drop_index.xml:55(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP INDEX [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:33(para) 
msgid "<command>DROP INDEX</command> drops an existing index from the database system. To execute this command you must be the owner of the index."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:47(para) 
msgid "Do not throw an error if the index does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:57(para) 
msgid "The name (optionally schema-qualified) of an index to remove."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:64(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:66(para) 
msgid "Automatically drop objects that depend on the index."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:73(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:75(para) 
msgid "Refuse to drop the index if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:85(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:90(programlisting) 
#, no-wrap
msgid "\nDROP INDEX title_idx;\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:87(para) 
msgid "This command will remove the index <literal>title_idx</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:96(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-QnGh/ref/drop_index.xml:98(para) 
msgid "<command>DROP INDEX</command> is a <productname>PostgreSQL</productname> language extension. There are no provisions for indexes in the SQL standard."
msgstr ""

