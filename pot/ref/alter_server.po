#: /tmp/cicero-LTnd/ref/alter_server.xml:25(synopsis) 
#, no-wrap
msgid "\nALTER SERVER <placeholder-1/> [ VERSION '<placeholder-2/>' ]\n    [ OPTIONS ( [ ADD | SET | DROP ] <placeholder-3/> ['<placeholder-4/>'] [, ... ] ) ]\nALTER SERVER <placeholder-5/> OWNER TO <placeholder-6/>\n"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:33(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:35(para) 
msgid "<command>ALTER SERVER</command> changes the definition of a foreign server. The first form changes the server version string or the generic options of the server (at least one clause is required). The second form changes the owner of the server."
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:42(para) 
msgid "To alter the server you must be the owner of the server. Additionally to alter the owner, you must own the server and also be a direct or indirect member of the new owning role, and you must have <literal>USAGE</literal> privilege on the server's foreign-data wrapper. (Note that superusers satisfy all these criteria automatically.)"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:53(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:59(para) 
msgid "The name of an existing server."
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:68(para) 
msgid "New server version."
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:75(literal) 
msgid "OPTIONS ( [ ADD | SET | DROP ] <placeholder-1/> ['<placeholder-2/>'] [, ... ] )"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:77(para) 
msgid "Change options for the server. <literal>ADD</literal>, <literal>SET</literal>, and <literal>DROP</literal> specify the action to be performed. <literal>ADD</literal> is assumed if no operation is explicitly specified. Option names must be unique; names and values are also validated using the server's foreign-data wrapper library."
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:91(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:95(programlisting) 
#, no-wrap
msgid "\nALTER SERVER foo OPTIONS (host 'foo', dbname 'foodb');\n"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:93(para) 
msgid "Alter server <literal>foo</literal>, add connection options: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:103(programlisting) 
#, no-wrap
msgid "\nALTER SERVER foo VERSION '8.4' OPTIONS (SET host 'baz');\n"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:100(para) 
msgid "Alter server <literal>foo</literal>, change version, change <literal>host</literal> option: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:109(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-LTnd/ref/alter_server.xml:111(para) 
msgid "<command>ALTER SERVER</command> conforms to ISO/IEC 9075-9 (SQL/MED)."
msgstr ""

