msgid "DROP FOREIGN DATA WRAPPER"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:16(refpurpose) 
msgid "remove a foreign-data wrapper"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:25(replaceable) /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:54(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP FOREIGN DATA WRAPPER [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:32(para) 
msgid "<command>DROP FOREIGN DATA WRAPPER</command> removes an existing foreign-data wrapper. To execute this command, the current user must be the owner of the foreign-data wrapper."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:44(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:46(para) 
msgid "Do not throw an error if the foreign-data wrapper does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:56(para) 
msgid "The name of an existing foreign-data wrapper."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:63(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:65(para) 
msgid "Automatically drop objects that depend on the foreign-data wrapper (such as servers)."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:73(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:75(para) 
msgid "Refuse to drop the foreign-data wrappers if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:85(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:89(programlisting) 
#, no-wrap
msgid "\nDROP FOREIGN DATA WRAPPER dbi;\n"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:87(para) 
msgid "Drop the foreign-data wrapper <literal>dbi</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:95(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-FQJK/ref/drop_foreign_data_wrapper.xml:97(para) 
msgid "<command>DROP FOREIGN DATA WRAPPER</command> conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal> clause is a <productname>PostgreSQL</productname> extension."
msgstr ""

