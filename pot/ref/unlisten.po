msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:32(para)
msgid "<command>UNLISTEN</command> is used to remove an existing registration for <command>NOTIFY</command> events. <command>UNLISTEN</command> cancels any existing registration of the current <productname>PostgreSQL</productname> session as a listener on the notification channel named <replaceable class=\"PARAMETER\">channel</replaceable>. The special wildcard <literal>*</literal> cancels all listener registrations for the current session."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:43(para)
msgid "<xref linkend=\"sql-notify\"/> contains a more extensive discussion of the use of <command>LISTEN</command> and <command>NOTIFY</command>."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:52(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:58(para)
msgid "Name of a notification channel (any identifier)."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:65(literal)
msgid "*"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:67(para)
msgid "All current listen registrations for this session are cleared."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:76(title)
msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:78(para)
msgid "You can unlisten something you were not listening for; no warning or error will appear."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:83(para)
msgid "At the end of each session, <command>UNLISTEN *</command> is automatically executed."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:88(para)
msgid "A transaction that has executed <command>UNLISTEN</command> cannot be prepared for two-phase commit."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/unlisten.xml:95(title)
msgid "Examples"
msgstr ""

