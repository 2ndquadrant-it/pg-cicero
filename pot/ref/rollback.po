#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:11(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:16(refpurpose)
msgid "abort the current transaction"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:24(synopsis)
#, no-wrap
msgid "\nROLLBACK [ WORK | TRANSACTION ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:32(para)
msgid "<command>ROLLBACK</command> rolls back the current transaction and causes all the updates made by the transaction to be discarded."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:39(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:43(literal)
msgid "WORK"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:44(literal)
msgid "TRANSACTION"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:46(para)
msgid "Optional key words. They have no effect."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:55(title)
msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:57(para)
msgid "Use <xref linkend=\"SQL-COMMIT\"/> to successfully terminate a transaction."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:62(para)
msgid "Issuing <command>ROLLBACK</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:69(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/rollback.xml:73(programlisting)
