msgid "7"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:11(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:16(refpurpose)
msgid "remove a procedural language"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:25(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:55(replaceable)
msgid "name"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:24(synopsis)
#, no-wrap
msgid "\nDROP [ PROCEDURAL ] LANGUAGE [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:32(para)
msgid "<command>DROP LANGUAGE</command> removes the definition of a previously registered procedural language. You must be a superuser or the owner of the language to use <command>DROP LANGUAGE</command>."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:40(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:45(literal)
msgid "IF EXISTS"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:47(para)
msgid "Do not throw an error if the language does not exist. A notice is issued in this case."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:57(para)
msgid "The name of an existing procedural language. For backward compatibility, the name can be enclosed by single quotes."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:65(literal)
msgid "CASCADE"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:67(para)
msgid "Automatically drop objects that depend on the language (such as functions in the language)."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:75(literal)
msgid "RESTRICT"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:77(para)
msgid "Refuse to drop the language if any objects depend on it. This is the default."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:87(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:93(programlisting)
#, no-wrap
msgid "\nDROP LANGUAGE plsample;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:89(para)
msgid "This command removes the procedural language <literal>plsample</literal>: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:100(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_language.xml:102(para)
msgid "There is no <command>DROP LANGUAGE</command> statement in the SQL standard."
msgstr ""

