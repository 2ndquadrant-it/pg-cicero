msgid "DROP TEXT SEARCH PARSER"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:16(refpurpose) 
msgid "remove a text search parser"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:25(replaceable) /tmp/cicero-CbEv/ref/drop_tsparser.xml:54(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP TEXT SEARCH PARSER [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:32(para) 
msgid "<command>DROP TEXT SEARCH PARSER</command> drops an existing text search parser. You must be a superuser to use this command."
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:39(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:44(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:46(para) 
msgid "Do not throw an error if the text search parser does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:56(para) 
msgid "The name (optionally schema-qualified) of an existing text search parser."
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:63(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:65(para) 
msgid "Automatically drop objects that depend on the text search parser."
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:72(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:74(para) 
msgid "Refuse to drop the text search parser if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:84(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:89(programlisting) 
#, no-wrap
msgid "\nDROP TEXT SEARCH PARSER my_parser;\n"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:86(para) 
msgid "Remove the text search parser <literal>my_parser</literal>: <placeholder-1/> This command will not succeed if there are any existing text search configurations that use the parser. Add <literal>CASCADE</literal> to drop such configurations along with the parser."
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:100(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-CbEv/ref/drop_tsparser.xml:102(para) 
msgid "There is no <command>DROP TEXT SEARCH PARSER</command> statement in the SQL standard."
msgstr ""

