#: /tmp/cicero-EpiG/ref/drop_language.xml:10(refentrytitle) /tmp/cicero-EpiG/ref/drop_language.xml:16(refname) /tmp/cicero-EpiG/ref/drop_language.xml:21(primary) 
msgid "DROP LANGUAGE"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:17(refpurpose) 
msgid "remove a procedural language"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:26(replaceable) /tmp/cicero-EpiG/ref/drop_language.xml:65(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP [ PROCEDURAL ] LANGUAGE [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:33(para) 
msgid "<command>DROP LANGUAGE</command> removes the definition of a previously registered procedural language. You must be a superuser or the owner of the language to use <command>DROP LANGUAGE</command>."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:40(para) 
msgid "As of <productname>PostgreSQL</productname> 9.1, most procedural languages have been made into <quote>extensions</quote>, and should therefore be removed with <xref linkend=\"sql-dropextension\"/> not <command>DROP LANGUAGE</command>."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:50(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:55(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:57(para) 
msgid "Do not throw an error if the language does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:67(para) 
msgid "The name of an existing procedural language. For backward compatibility, the name can be enclosed by single quotes."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:75(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:77(para) 
msgid "Automatically drop objects that depend on the language (such as functions in the language)."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:85(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:87(para) 
msgid "Refuse to drop the language if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:97(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:103(programlisting) 
#, no-wrap
msgid "\nDROP LANGUAGE plsample;\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:99(para) 
msgid "This command removes the procedural language <literal>plsample</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:109(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_language.xml:111(para) 
msgid "There is no <command>DROP LANGUAGE</command> statement in the SQL standard."
msgstr ""

