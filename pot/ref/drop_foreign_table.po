msgid "DROP FOREIGN TABLE"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:8(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:9(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:14(refpurpose) 
msgid "remove a foreign table"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:23(replaceable) /tmp/cicero-EpiG/ref/drop_foreign_table.xml:51(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:22(synopsis) 
#, no-wrap
msgid "\nDROP FOREIGN TABLE [ IF EXISTS ] <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:28(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:30(para) 
msgid "<command>DROP FOREIGN TABLE</command> removes a foreign table. Only the owner of a foreign table can remove it."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:37(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:41(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:43(para) 
msgid "Do not throw an error if the foreign table does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:53(para) 
msgid "The name (optionally schema-qualified) of the foreign table to drop."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:60(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:62(para) 
msgid "Automatically drop objects that depend on the foreign table (such as views)."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:70(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:72(para) 
msgid "Refuse to drop the foreign table if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:82(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:88(programlisting) 
#, no-wrap
msgid "\nDROP FOREIGN TABLE films, distributors;\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:84(para) 
msgid "To destroy two foreign tables, <literal>films</literal> and <literal>distributors</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:94(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-EpiG/ref/drop_foreign_table.xml:96(para) 
msgid "This command conforms to the ISO/IEC 9075-9 (SQL/MED), except that the standard only allows one foreign table to be dropped per command, and apart from the <literal>IF EXISTS</literal> option, which is a <productname>PostgreSQL</productname> extension."
msgstr ""

