msgid "\nSELECT date_part('day', TIMESTAMP '2001-02-16 20:38:40');\n<placeholder-1/><placeholder-2/>\n\nSELECT date_part('hour', INTERVAL '4 hours 3 minutes');\n<placeholder-3/><placeholder-4/>\n"
