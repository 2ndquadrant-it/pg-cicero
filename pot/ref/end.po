msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/end.xml:47(literal)
msgid "TRANSACTION"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/end.xml:49(para)
msgid "Optional key words. They have no effect."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/end.xml:58(title)
