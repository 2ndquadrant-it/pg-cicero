#: /tmp/cicero-EpiG/ref/alter_trigger.xml:25(synopsis) 
#, no-wrap
msgid "\nALTER TRIGGER <placeholder-1/> ON <placeholder-2/> RENAME TO <placeholder-3/>\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:33(para) 
msgid "<command>ALTER TRIGGER</command> changes properties of an existing trigger. The <literal>RENAME</literal> clause changes the name of the given trigger without otherwise changing the trigger definition."
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:40(para) 
msgid "You must own the table on which the trigger acts to be allowed to change its properties."
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:46(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:52(para) 
msgid "The name of an existing trigger to alter."
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:61(para) 
msgid "The name of the table on which this trigger acts."
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:70(para) 
msgid "The new name for the trigger."
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:79(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:81(para) 
msgid "The ability to temporarily enable or disable a trigger is provided by <xref linkend=\"sql-altertable\"/>, not by <command>ALTER TRIGGER</command>, because <command>ALTER TRIGGER</command> has no convenient way to express the option of enabling or disabling all of a table's triggers at once."
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:91(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:95(programlisting) 
#, no-wrap
msgid "\nALTER TRIGGER emp_stamp ON emp RENAME TO emp_track_chgs;\n"
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:93(para) 
msgid "To rename an existing trigger: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:101(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-EpiG/ref/alter_trigger.xml:103(para) 
msgid "<command>ALTER TRIGGER</command> is a <productname>PostgreSQL</productname> extension of the SQL standard."
msgstr ""

