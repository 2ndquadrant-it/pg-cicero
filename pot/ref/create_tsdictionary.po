#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:25(synopsis) 
#, no-wrap
msgid "\nCREATE TEXT SEARCH DICTIONARY <placeholder-1/> (\n    TEMPLATE = <placeholder-2/>\n    [, <placeholder-3/> = <placeholder-4/> [, ... ]]\n)\n"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:34(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:36(para) 
msgid "<command>CREATE TEXT SEARCH DICTIONARY</command> creates a new text search dictionary. A text search dictionary specifies a way of recognizing interesting or uninteresting words for searching. A dictionary depends on a text search template, which specifies the functions that actually perform the work. Typically the dictionary provides some options that control the detailed behavior of the template's functions."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:45(para) 
msgid "If a schema name is given then the text search dictionary is created in the specified schema. Otherwise it is created in the current schema."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:50(para) 
msgid "The user who defines a text search dictionary becomes its owner."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:54(para) 
msgid "Refer to <xref linkend=\"textsearch\"/> for further information."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:60(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:66(para) 
msgid "The name of the text search dictionary to be created. The name can be schema-qualified."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:76(para) 
msgid "The name of the text search template that will define the basic behavior of this dictionary."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:86(para) 
msgid "The name of a template-specific option to be set for this dictionary."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:95(para) 
msgid "The value to use for a template-specific option. If the value is not a simple identifier or number, it must be quoted (but you can always quote it, if you wish)."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:104(para) 
msgid "The options can appear in any order."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:110(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:112(para) 
msgid "The following example command creates a Snowball-based dictionary with a nonstandard list of stop words."
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:117(programlisting) 
#, no-wrap
msgid "\nCREATE TEXT SEARCH DICTIONARY my_russian (\n    template = snowball,\n    language = russian,\n    stopwords = myrussian\n);\n"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:127(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-LTnd/ref/create_tsdictionary.xml:129(para) 
msgid "There is no <command>CREATE TEXT SEARCH DICTIONARY</command> statement in the SQL standard."
msgstr ""

