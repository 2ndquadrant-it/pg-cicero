msgid "ROLLBACK"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:16(refpurpose) 
msgid "abort the current transaction"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:24(synopsis) 
#, no-wrap
msgid "\nROLLBACK [ WORK | TRANSACTION ]\n"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:32(para) 
msgid "<command>ROLLBACK</command> rolls back the current transaction and causes all the updates made by the transaction to be discarded."
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:39(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:43(literal) 
msgid "WORK"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:44(literal) 
msgid "TRANSACTION"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:46(para) 
msgid "Optional key words. They have no effect."
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:55(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:57(para) 
msgid "Use <xref linkend=\"sql-commit\"/> to successfully terminate a transaction."
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:62(para) 
msgid "Issuing <command>ROLLBACK</command> when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:69(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:73(programlisting) 
#, no-wrap
msgid "\nROLLBACK;\n"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:71(para) 
msgid "To abort all changes: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:79(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-eMpW/ref/rollback.xml:81(para) 
msgid "The SQL standard only specifies the two forms <literal>ROLLBACK</literal> and <literal>ROLLBACK WORK</literal>. Otherwise, this command is fully conforming."
msgstr ""

