msgstr ""

#: /tmp/cicero-RKYt/ref/drop_cast.xml:49(para) 
msgid "Do not throw an error if the cast does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_cast.xml:60(para) 
msgid "The name of the source data type of the cast."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_cast.xml:70(para) 
msgid "The name of the target data type of the cast."
msgstr ""

