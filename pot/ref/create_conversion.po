msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:28(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:30(para)
msgid "<command>CREATE CONVERSION</command> defines a new conversion between character set encodings. Also, conversions that are marked <literal>DEFAULT</literal> can be used for automatic encoding conversion between client and server. For this purpose, two conversions, from encoding A to B <emphasis>and</emphasis> from encoding B to A, must be defined."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:39(para)
msgid "To be able to create a conversion, you must have <literal>EXECUTE</literal> privilege on the function and <literal>CREATE</literal> privilege on the destination schema."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:47(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:51(literal)
msgid "DEFAULT"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:54(para)
msgid "The <literal>DEFAULT</literal> clause indicates that this conversion is the default for this particular source to destination encoding. There should be only one default encoding in a schema for the encoding pair."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:67(para)
msgid "The name of the conversion. The conversion name can be schema-qualified. If it is not, the conversion is defined in the current schema. The conversion name must be unique within a schema."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:80(para)
msgid "The source encoding name."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:90(para)
msgid "The destination encoding name."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:100(para)
msgid "The function used to perform the conversion. The function name can be schema-qualified. If it is not, the function will be looked up in the path."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:109(programlisting)
#, no-wrap
msgid "\nconv_proc(\n    integer,  -- source encoding ID\n    integer,  -- destination encoding ID\n    cstring,  -- source string (null terminated C string)\n    internal, -- destination (fill with a null terminated C string)\n    integer   -- source string length\n) RETURNS void;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/create_conversion.xml:106(para)
msgid "The function must have the following signature: <placeholder-1/>"
msgstr ""

