#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:34(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:36(para)
msgid "<application>dropdb</application> destroys an existing <productname>PostgreSQL</productname> database. The user who executes this command must be a database superuser or the owner of the database."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:43(para)
msgid "<application>dropdb</application> is a wrapper around the <acronym>SQL</acronym> command <xref linkend=\"SQL-DROPDATABASE\"/>. There is no effective difference between dropping databases via this utility and via other methods for accessing the server."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:54(title)
msgid "Options"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:63(para)
msgid "Specifies the name of the database to be removed."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:70(option)
msgid "-e"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:71(option)
msgid "--echo"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:73(para)
msgid "Echo the commands that <application>dropdb</application> generates and sends to the server."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:81(option)
msgid "-i"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:82(option)
msgid "--interactive"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:84(para)
msgid "Issues a verification prompt before doing anything destructive."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:91(option)
msgid "-V"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:92(option)
msgid "--version"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:94(para)
msgid "Print the <application>dropdb</application> version and exit."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:101(option)
msgid "-?"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:102(option)
msgid "--help"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:104(para)
msgid "Show help about <application>dropdb</application> command line arguments, and exit."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:56(para)
msgid "<application>dropdb</application> accepts the following command-line arguments: <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:121(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:122(replaceable)
msgid "host"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:121(option)
msgid "-h <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:122(option)
msgid "--host <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:124(para)
msgid "Specifies the host name of the machine on which the server is running. If the value begins with a slash, it is used as the directory for the Unix domain socket."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:134(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:135(replaceable)
msgid "port"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:134(option)
msgid "-p <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:135(option)
msgid "--port <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:137(para)
msgid "Specifies the TCP port or local Unix domain socket file extension on which the server is listening for connections."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:146(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:147(replaceable)
msgid "username"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:146(option)
msgid "-U <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:147(option)
msgid "--username <placeholder-1/>"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:149(para)
msgid "User name to connect as."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:156(option)
msgid "-w"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:157(option)
msgid "--no-password"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:159(para)
msgid "Never issue a password prompt. If the server requires password authentication and a password is not available by other means such as a <filename>.pgpass</filename> file, the connection attempt will fail. This option can be useful in batch jobs and scripts where no user is present to enter a password."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:171(option)
msgid "-W"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:172(option)
msgid "--password"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:174(para)
msgid "Force <application>dropdb</application> to prompt for a password before connecting to a database."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/dropdb.xml:179(para)
msgid "This option is never essential, since <application>dropdb</application> will automatically prompt for a password if the server demands password authentication. However, <application>dropdb</application> will waste a connection attempt finding out that the server wants a password. In some cases it is worth typing <option>-W</option> to avoid the extra connection attempt."
msgstr ""

