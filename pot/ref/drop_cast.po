#: /tmp/cicero-dOlG/ref/drop_cast.xml:21(synopsis) 
#, no-wrap
msgid "\nDROP CAST [ IF EXISTS ] (<placeholder-1/> AS <placeholder-2/>) [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:27(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:29(para) 
msgid "<command>DROP CAST</command> removes a previously defined cast."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:33(para) 
msgid "To be able to drop a cast, you must own the source or the target data type. These are the same privileges that are required to create a cast."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:46(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:48(para) 
msgid "Do not throw an error if the cast does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:59(para) 
msgid "The name of the source data type of the cast."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:69(para) 
msgid "The name of the target data type of the cast."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:76(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:77(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:80(para) 
msgid "These key words do not have any effect, since there are no dependencies on casts."
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:90(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:94(programlisting) 
#, no-wrap
msgid "\nDROP CAST (text AS int);\n"
msgstr ""

#: /tmp/cicero-dOlG/ref/drop_cast.xml:92(para) 
msgid "To drop the cast from type <type>text</type> to type <type>int</type>: <placeholder-1/>"
msgstr ""

