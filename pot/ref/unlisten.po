msgid "UNLISTEN"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:16(refpurpose) 
msgid "stop listening for a notification"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:25(replaceable) /tmp/cicero-dOlG/ref/unlisten.xml:56(replaceable) 
msgid "channel"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:24(synopsis) 
#, no-wrap
msgid "\nUNLISTEN { <placeholder-1/> | * }\n"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:32(para) 
msgid "<command>UNLISTEN</command> is used to remove an existing registration for <command>NOTIFY</command> events. <command>UNLISTEN</command> cancels any existing registration of the current <productname>PostgreSQL</productname> session as a listener on the notification channel named <replaceable class=\"PARAMETER\">channel</replaceable>. The special wildcard <literal>*</literal> cancels all listener registrations for the current session."
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:43(para) 
msgid "<xref linkend=\"sql-notify\"/> contains a more extensive discussion of the use of <command>LISTEN</command> and <command>NOTIFY</command>."
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:52(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:58(para) 
msgid "Name of a notification channel (any identifier)."
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:65(literal) 
msgid "*"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:67(para) 
msgid "All current listen registrations for this session are cleared."
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:76(title) 
msgid "Notes"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:78(para) 
msgid "You can unlisten something you were not listening for; no warning or error will appear."
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:83(para) 
msgid "At the end of each session, <command>UNLISTEN *</command> is automatically executed."
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:88(para) 
msgid "A transaction that has executed <command>UNLISTEN</command> cannot be prepared for two-phase commit."
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:95(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:100(programlisting) 
#, no-wrap
msgid "\nLISTEN virtual;\nNOTIFY virtual;\nAsynchronous notification \"virtual\" received from server process with PID 8448.\n"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:97(para) 
msgid "To make a registration: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:111(programlisting) 
#, no-wrap
msgid "\nUNLISTEN virtual;\nNOTIFY virtual;\n-- no NOTIFY event is received\n"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:107(para) 
msgid "Once <command>UNLISTEN</command> has been executed, further <command>NOTIFY</command> messages will be ignored: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:119(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-dOlG/ref/unlisten.xml:121(para) 
msgid "There is no <command>UNLISTEN</command> command in the SQL standard."
msgstr ""

