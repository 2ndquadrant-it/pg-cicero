#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:16(refpurpose)
msgid "remove a text search template"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:25(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:54(replaceable)
msgid "name"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:24(synopsis)
#, no-wrap
msgid "\nDROP TEXT SEARCH TEMPLATE [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:32(para)
msgid "<command>DROP TEXT SEARCH TEMPLATE</command> drops an existing text search template. You must be a superuser to use this command."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:39(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:44(literal)
msgid "IF EXISTS"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:46(para)
msgid "Do not throw an error if the text search template does not exist. A notice is issued in this case."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:56(para)
msgid "The name (optionally schema-qualified) of an existing text search template."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:64(literal)
msgid "CASCADE"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:66(para)
msgid "Automatically drop objects that depend on the text search template."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:73(literal)
msgid "RESTRICT"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:75(para)
msgid "Refuse to drop the text search template if any objects depend on it. This is the default."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:85(title)
msgid "Examples"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:90(programlisting)
#, no-wrap
msgid "\nDROP TEXT SEARCH TEMPLATE thesaurus;\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:87(para)
msgid "Remove the text search template <literal>thesaurus</literal>: <placeholder-1/> This command will not succeed if there are any existing text search dictionaries that use the template. Add <literal>CASCADE</literal> to drop such dictionaries along with the template."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:101(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_tstemplate.xml:103(para)
msgid "There is no <command>DROP TEXT SEARCH TEMPLATE</command> statement in the SQL standard."
msgstr ""

