#: /tmp/cicero-QnGh/ref/alter_collation.xml:25(synopsis) 
#, no-wrap
msgid "\nALTER COLLATION <placeholder-1/> RENAME TO <placeholder-2/>\nALTER COLLATION <placeholder-3/> OWNER TO <placeholder-4/>\nALTER COLLATION <placeholder-5/> SET SCHEMA <placeholder-6/>\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:33(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:35(para) 
msgid "<command>ALTER COLLATION</command> changes the definition of a collation."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:40(para) 
msgid "You must own the collation to use <command>ALTER COLLATION</command>. To alter the owner, you must also be a direct or indirect member of the new owning role, and that role must have <literal>CREATE</literal> privilege on the collation's schema. (These restrictions enforce that altering the owner doesn't do anything you couldn't do by dropping and recreating the collation. However, a superuser can alter ownership of any collation anyway.)"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:52(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:58(para) 
msgid "The name (optionally schema-qualified) of an existing collation."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:67(para) 
msgid "The new name of the collation."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:76(para) 
msgid "The new owner of the collation."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:85(para) 
msgid "The new schema for the collation."
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:94(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:99(programlisting) 
#, no-wrap
msgid "\nALTER COLLATION \"de_DE\" RENAME TO german;\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:96(para) 
msgid "To rename the collation <literal>de_DE</literal> to <literal>german</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:107(programlisting) 
#, no-wrap
msgid "\nALTER COLLATION \"en_US\" OWNER TO joe;\n"
msgstr ""

#: /tmp/cicero-QnGh/ref/alter_collation.xml:104(para) 
msgid "To change the owner of the collation <literal>en_US</literal> to <literal>joe</literal>: <placeholder-1/>"
msgstr ""

