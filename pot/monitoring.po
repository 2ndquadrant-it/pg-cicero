#: /tmp/cicero-jWWW/monitoring.xml:444(structname) /tmp/cicero-jWWW/monitoring.xml:444(primary) /tmp/cicero-jWWW/monitoring.xml:453(structname) /tmp/cicero-jWWW/monitoring.xml:460(structname) 
msgid "pg_statio_all_sequences"
msgstr ""

#: /tmp/cicero-jWWW/monitoring.xml:445(entry) 
msgid "For each sequence object in the current database, the sequence OID, schema and sequence name, numbers of disk blocks read and buffer hits in that sequence."
msgstr ""

#: /tmp/cicero-jWWW/monitoring.xml:452(structname) /tmp/cicero-jWWW/monitoring.xml:452(primary) 
msgid "pg_statio_sys_sequences"
msgstr ""

#: /tmp/cicero-jWWW/monitoring.xml:453(entry) 
