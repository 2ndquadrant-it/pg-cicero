msgid "-e"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:97(option) 
msgid "--echo"
msgstr ""

#: /tmp/cicero-hjxh/ref/clusterdb.xml:99(para) 
msgid "Echo the commands that <application>clusterdb</application> generates and sends to the server."
msgstr ""

