msgid "7"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:11(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:16(refpurpose)
msgid "remove database objects owned by a database role"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:25(replaceable) /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:45(replaceable)
msgid "name"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:24(synopsis)
#, no-wrap
msgid "\nDROP OWNED BY <placeholder-1/> [, ...] [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:30(title)
msgid "Description"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:32(para)
msgid "<command>DROP OWNED</command> drops all the objects in the current database that are owned by one of the specified roles. Any privileges granted to the given roles on objects in the current database will also be revoked."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:41(title)
msgid "Parameters"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:47(para)
msgid "The name of a role whose objects will be dropped, and whose privileges will be revoked."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:55(literal)
msgid "CASCADE"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:57(para)
msgid "Automatically drop objects that depend on the affected objects."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:64(literal)
msgid "RESTRICT"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:66(para)
msgid "Refuse to drop the objects owned by a role if any other database objects depend on one of the affected objects. This is the default."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:76(title)
msgid "Notes"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:77(para)
msgid "<command>DROP OWNED</command> is often used to prepare for the removal of one or more roles. Because <command>DROP OWNED</command> only affects the objects in the current database, it is usually necessary to execute this command in each database that contains objects owned by a role that is to be removed."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:85(para)
msgid "Using the <literal>CASCADE</literal> option might make the command recurse to objects owned by other users."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:90(para)
msgid "The <xref linkend=\"sql-reassign-owned\"/> command is an alternative that reassigns the ownership of all the database objects owned by one or more roles."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:96(para)
msgid "Databases owned by the role(s) will not be removed."
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:102(title)
msgid "Compatibility"
msgstr ""

#: /home/carloratm/progetti/postgresql-source/doc-9.0.4/ref/drop_owned.xml:104(para)
msgid "The <command>DROP OWNED</command> statement is a <productname>PostgreSQL</productname> extension."
msgstr ""

