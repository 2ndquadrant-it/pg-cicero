msgid "DROP FOREIGN DATA WRAPPER"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:11(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:12(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:17(refpurpose) 
msgid "remove a foreign-data wrapper"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:26(replaceable) /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:55(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:25(synopsis) 
#, no-wrap
msgid "\nDROP FOREIGN DATA WRAPPER [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:31(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:33(para) 
msgid "<command>DROP FOREIGN DATA WRAPPER</command> removes an existing foreign-data wrapper. To execute this command, the current user must be the owner of the foreign-data wrapper."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:41(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:47(para) 
msgid "Do not throw an error if the foreign-data wrapper does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:57(para) 
msgid "The name of an existing foreign-data wrapper."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:64(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:66(para) 
msgid "Automatically drop objects that depend on the foreign-data wrapper (such as servers)."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:74(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:76(para) 
msgid "Refuse to drop the foreign-data wrappers if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:86(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:90(programlisting) 
#, no-wrap
msgid "\nDROP FOREIGN DATA WRAPPER dbi;\n"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:88(para) 
msgid "Drop the foreign-data wrapper <literal>dbi</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:96(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-lkRf/ref/drop_foreign_data_wrapper.xml:98(para) 
msgid "<command>DROP FOREIGN DATA WRAPPER</command> conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal> clause is a <productname>PostgreSQL</productname> extension."
msgstr ""

