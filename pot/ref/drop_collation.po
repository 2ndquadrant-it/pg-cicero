msgid "DROP COLLATION"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:8(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:9(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:14(refpurpose) 
msgid "remove a collation"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:23(replaceable) /tmp/cicero-RKYt/ref/drop_collation.xml:51(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:22(synopsis) 
#, no-wrap
msgid "\nDROP COLLATION [ IF EXISTS ] <placeholder-1/> [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:28(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:30(para) 
msgid "<command>DROP COLLATION</command> removes a previously defined collation. To be able to drop a collation, you must own the collation."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:37(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:41(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:43(para) 
msgid "Do not throw an error if the collation does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:54(para) 
msgid "The name of the collation. The collation name can be schema-qualified."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:62(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:64(para) 
msgid "Automatically drop objects that depend on the collation."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:71(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:73(para) 
msgid "Refuse to drop the collation if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:83(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:87(programlisting) 
#, no-wrap
msgid "\nDROP COLLATION german;\n"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:85(para) 
msgid "To drop the collation named <literal>german</literal>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:93(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-RKYt/ref/drop_collation.xml:95(para) 
msgid "The <command>DROP COLLATION</command> command conforms to the <acronym>SQL</acronym> standard, apart from the <literal>IF EXISTS</literal> option, which is a <productname>PostgreSQL</productname> extension."
msgstr ""

