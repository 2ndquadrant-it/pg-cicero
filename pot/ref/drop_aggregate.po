msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:10(manvolnum) 
msgid "7"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:11(refmiscinfo) 
msgid "SQL - Language Statements"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:16(refpurpose) 
msgid "remove an aggregate function"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:25(replaceable) /tmp/cicero-VdKm/ref/drop_aggregate.xml:55(replaceable) 
msgid "name"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:25(replaceable) /tmp/cicero-VdKm/ref/drop_aggregate.xml:64(replaceable) 
msgid "type"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:24(synopsis) 
#, no-wrap
msgid "\nDROP AGGREGATE [ IF EXISTS ] <placeholder-1/> ( <placeholder-2/> [ , ... ] ) [ CASCADE | RESTRICT ]\n"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:30(title) 
msgid "Description"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:32(para) 
msgid "<command>DROP AGGREGATE</command> removes an existing aggregate function. To execute this command the current user must be the owner of the aggregate function."
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:40(title) 
msgid "Parameters"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:45(literal) 
msgid "IF EXISTS"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:47(para) 
msgid "Do not throw an error if the aggregate does not exist. A notice is issued in this case."
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:57(para) 
msgid "The name (optionally schema-qualified) of an existing aggregate function."
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:66(para) 
msgid "An input data type on which the aggregate function operates. To reference a zero-argument aggregate function, write <literal>*</literal> in place of the list of input data types."
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:75(literal) 
msgid "CASCADE"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:77(para) 
msgid "Automatically drop objects that depend on the aggregate function."
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:84(literal) 
msgid "RESTRICT"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:86(para) 
msgid "Refuse to drop the aggregate function if any objects depend on it. This is the default."
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:96(title) 
msgid "Examples"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:101(programlisting) 
#, no-wrap
msgid "\nDROP AGGREGATE myavg(integer);\n"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:98(para) 
msgid "To remove the aggregate function <literal>myavg</literal> for type <type>integer</type>: <placeholder-1/>"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:107(title) 
msgid "Compatibility"
msgstr ""

#: /tmp/cicero-VdKm/ref/drop_aggregate.xml:109(para) 
msgid "There is no <command>DROP AGGREGATE</command> statement in the SQL standard."
msgstr ""

